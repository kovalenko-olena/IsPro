    <TfrxODBCQuery Name="ACCSE" UserName="ACCSE" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="SELECT PayAccSE.PayAcc_SE&#13;&#10;FROM PayAccSE&#13;&#10;WHERE PayAccSE.PayAcc_Role = 0&#13;&#10;AND PayAccSE.PayAcc_Usr = :PayAcc_Usr&#13;&#10;AND PayAccSE.PayAcc_IsKdr=1&#13;&#10;UNION ALL&#13;&#10;SELECT PayAccSE.PayAcc_SE&#13;&#10;FROM PayAccSE&#13;&#10;WHERE PayAccSE.PayAcc_Role = 1&#13;&#10;AND PayAcc_IsKdr=1&#13;&#10;AND PayAccSE.PayAcc_Usr&#13;&#10;IN (&#13;&#10;     SELECT UserRole_Rcd&#13;&#10;     FROM UserToRole&#13;&#10;     WHERE User_Rcd = :PayAcc_Usr1&#13;&#10;     AND UserRole_Firm = :CdFirm&#13;&#10;   )" PreprocessSQLText="true" SQLZeroCols="false" PropData=""/>
