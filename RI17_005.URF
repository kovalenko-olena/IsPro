<TfrxReport Version="5.4.3" DotMatrixReport="false" IniFile="\Software\Fast Reports" OldStyleProgress="true" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Yurii Marchenko" ReportOptions.CreateDate="38132,6247705093" ReportOptions.Description.Text="" ReportOptions.Name="FR ЮГОК Повiдомлення про змiну облiкових даних" ReportOptions.LastChange="45028,4536443403" ReportOptions.Law="rpwUA" ReportOptions.Language="rpgUA" ReportOptions.DirectExportType="deRtf" ReportOptions.DirectExportMode="dmOpen" ReportOptions.DirectExportPages="depSingleSheet" ReportOptions.DirectExportChunkSize="0" ScriptLanguage="PascalScript" ScriptText.Text="const mrk_cd=150; frm_date='dd.mm.yyyy';&#13;&#10;&#13;&#10;VAR SignaturePodpisCd:string;&#13;&#10;    ReasonText:string;&#13;&#10;    osn:string;&#13;&#10;&#13;&#10;&#13;&#10;function GetIF(fio:string) :string;&#13;&#10;begin&#13;&#10;  if Length(BPRO.GETSHORTIF(fio, 0)) &#62; 0 then&#13;&#10;    result := BPRO.GETSHORTIF(fio, 0)&#13;&#10;  else&#13;&#10;    result := fio;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetDateParse(dt : TDateTime):string;&#13;&#10;begin&#13;&#10;  if YearOf(dt) &#62; 1900 then&#13;&#10;    result := FormatDateTime('DD.MM.YYYY',dt)&#13;&#10;  else&#13;&#10;    result := '';&#13;&#10;end;&#13;&#10;&#13;&#10;//---------------------&#13;&#10;function Replace (str, find, repl: string; allFind:boolean=true):string;&#13;&#10;var buf1, buf2, buffer : string;&#13;&#10;begin&#13;&#10;  buf1:='';&#13;&#10;  buf2:=str;&#13;&#10;  buffer:=str;&#13;&#10;  while Pos(find,buf2)&#62;0 do&#13;&#10;  begin&#13;&#10;    buf2:=copy(buf2, Pos(find,buf2), (Length(buf2)-Pos(find,buf2))+1);&#13;&#10;    buf1:=Copy(buffer, 1, Length(buffer)-Length(buf2))+repl;&#13;&#10;    Delete(buf2, Pos(find,buf2), Length(find));&#13;&#10;    buffer:=buf1+buf2;&#13;&#10;    if not allFind then break;&#13;&#10;  end;&#13;&#10;  Result:=buffer;&#13;&#10;end;&#13;&#10;//--------------------------------------------------------------------------------------------------&#13;&#10;// Заменяет SQL шаблон в запросе&#13;&#10;// v1.0&#13;&#10;procedure xstr_ReplaceSqlParam(&#13;&#10;    var source: TfrxODBCQuery;&#13;&#10;    oldPattern,&#13;&#10;    newPattern: string);&#13;&#10;begin&#13;&#10;    source.SQL.Text:= Replace(source.SQL.Text, oldPattern, newPattern);&#13;&#10;end;&#13;&#10;//----------------------------------------------------&#13;&#10;// заполнение LookupComboBox c выботром сохраненного индекса&#13;&#10;procedure loadLookupCB(var lcbSource: TfrxDBLookupComboBox; selIndex:string='');&#13;&#10;begin&#13;&#10;  lcbSource.DataSet.Open;&#13;&#10;  lcbSource.DataSet.First;&#13;&#10;  If (selIndex&#60;&#62;'') and ValidInt(selIndex) then&#13;&#10;      lcbSource.KeyValue:=StrToInt(selIndex);&#13;&#10;end;&#13;&#10;&#13;&#10;// --------------------------------------------------------------------------------------------------&#13;&#10;// _CheckStayOK - проверка возможности печати отчета&#13;&#10;// example&#13;&#10;procedure _CheckStayOK();&#13;&#10;begin&#13;&#10;    {If&#13;&#10;       (lcbPodpisDol.KeyValue=null)&#13;&#10;    Then&#13;&#10;       btOK.Enabled:=false&#13;&#10;    Else&#13;&#10;       btOK.Enabled:=true;  }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure StopReport(msgText:string='Звіт порожній');&#13;&#10;begin&#13;&#10;     ShowMessage(msgText);&#13;&#10;     StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  BPRO.DEL_MRK_SRV(mrk_cd);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetLastUserParams();&#13;&#10;begin&#13;&#10;  SetUserRepParam('DocCd',edDocCd.Text);&#13;&#10;  SetUserRepParam('EdrCd',edEdrCd.Text);&#13;&#10;  SetUserRepParam('DocDate',DateToStr(dDocDate.Date));&#13;&#10;  SetUserRepParam('SignaturePodpisCd',SignaturePodpisCd);&#13;&#10;  SetUserRepParam('Signature',edSignature.Text);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GetLastUserParams();&#13;&#10;begin&#13;&#10;  edDocCd.Text:=  GetUserRepParam('DocCd');&#13;&#10;  edEdrCd.Text:=  GetUserRepParam('EdrCd');&#13;&#10;  dDocDate.Date:=iif(ValidDate(GetUserRepParam('DocDate')),StrToDate(GetUserRepParam('DocDate')),Date());&#13;&#10;  SignaturePodpisCd:=GetUserRepParam('SignaturePodpisCd');&#13;&#10;  loadLookupCB(lcbPodpisDol,SignaturePodpisCd);&#13;&#10;  edSignature.Text:=GetUserRepParam('Signature');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure lcbPodpisDolOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if not lcbPodpisDol.DataSet.Eof then&#13;&#10;    begin&#13;&#10;      TEXT.Close;&#13;&#10;      TEXT.Open;&#13;&#10;      edSignature.Text:=&#60;TEXT.&#34;KDRPPS_FIO&#34;&#62;;&#13;&#10;      TEXT.Close;&#13;&#10;      SignaturePodpisCd:=VarToStr(lcbPodpisDol.KeyValue);&#13;&#10;      _CheckStayOK();&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure CheckPlaceRun();&#13;&#10;var str_sql: string; CD_MD:integer;&#13;&#10;begin&#13;&#10;  CD_MD:=BPRO.BPRO_CD_MD();&#13;&#10;  if (CD_MD in [189, 513]) then // Запуск из личных карточек кадров 521&#13;&#10;  begin&#13;&#10;    GetDsMark(mrk_cd);&#13;&#10;    dsMark.First;&#13;&#10;    if dsMark.EOF then&#13;&#10;    begin&#13;&#10;       dsBaseInfo.Locate( 'Name', 'KPUX', loCaseInsensitive);&#13;&#10;       str_sql := str_sql + 'WHERE KpuC1.Kpu_Rcd = '+ IntToStr(&#60;dsBaseInfo.&#34;RCD&#34;&#62;) + Chr(13);&#13;&#10;    end else&#13;&#10;    begin&#13;&#10;       BPRO.PUT_MRK_SRV(mrk_cd);&#13;&#10;       str_sql := str_sql + 'WHERE KpuC1.Kpu_Rcd IN (SELECT ServerMrk_Rcd FROM ServerMrk WHERE ServerMrk_Type = '+ IntToStr(mrk_cd) +' AND ServerMrk_Sess = '+IntToStr(GetStationCd())+')' + Chr(13);&#13;&#10;       str_sql := str_sql + 'order by kpuc1.kpu_fio';&#13;&#10;    end;&#13;&#10;    qSoldier.SQL.Add(str_sql);&#13;&#10;  end else&#13;&#10;  StopReport('Звіт працює лише з особових карток кадрів!');//+IntToStr(BPRO.BPRO_CD_MD()));// ');//&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MainOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   PreparePodpis();&#13;&#10;   GetLastUserParams();&#13;&#10;   _CheckStayOK();&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetLastUserParams();&#13;&#10;end;&#13;&#10;&#13;&#10;function GetDateUFormat(frmDate:date):string;&#13;&#10;var sYear,sYear1,sYear2,sMonth, sDay, frm:string;&#13;&#10;begin&#13;&#10;   frm:='&#60;u&#62; %s &#60;/u&#62; &#60;u&#62; %s &#60;/u&#62; %s&#60;u&#62;%s&#60;/u&#62;';&#13;&#10;   if frmDate=0 then&#13;&#10;     begin&#13;&#10;       sDay:='   ';&#13;&#10;       sMonth:='         ';&#13;&#10;       sYear1:='20';&#13;&#10;       sYear2:='  ';&#13;&#10;     end&#13;&#10;   else&#13;&#10;     begin&#13;&#10;       sDay:=IntToStr(DayOf(frmDate));&#13;&#10;       sMonth:=BPRO.FMONTH_NM_PAD(frmDate,0,1);&#13;&#10;       sYear:=IntToStr(YearOf(frmDate));&#13;&#10;       sYear1:=Copy(sYear,1,2);&#13;&#10;       sYear2:=Copy(sYear,3,2);&#13;&#10;     end;&#13;&#10;   Result:=Format(frm,[sDay,sMonth,sYear1,sYear2]);&#13;&#10;end;&#13;&#10;&#13;&#10;function GetWarDocText():string;&#13;&#10; var docSerNum,docFormatDate: string;&#13;&#10;begin&#13;&#10;  //docSerNum:='TY 4554';&#13;&#10;  docSerNum:=trim(&#60;qSoldier.&#34;KpuWar_DocN&#34;&#62;);&#13;&#10;  if (docSerNum&#60;&#62;'') then&#13;&#10;   begin&#13;&#10;     docSerNum := iif(pos(' ',docSerNum) = 0 , '&#60;u&#62;     &#60;/u&#62; № ' , '') + '&#60;u&#62; '+ replace(docSerNum, ' ',' &#60;/u&#62; № &#60;u&#62; ',false)+' &#60;/u&#62;' ;&#13;&#10;   end&#13;&#10;  else&#13;&#10;   docSerNum:='&#60;u&#62;     &#60;/u&#62; № &#60;u&#62;          &#60;/u&#62;';&#13;&#10;&#13;&#10;  docFormatDate:=GetDateUFormat(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;)+'р.';&#13;&#10;  Result:=Format( '%s, виданий  %s',[docSerNum,docFormatDate]);&#13;&#10;end;&#13;&#10;&#13;&#10;function GetWarDocSerOrNum(isSer:boolean=true;):string;&#13;&#10;var docSerNum, ser, num : string;&#13;&#10;var p : integer;&#13;&#10;begin&#13;&#10;&#13;&#10;  //docSerNum := 'RE5 dDDDDD';&#13;&#10;  docSerNum := trim(&#60;qSoldier.&#34;KpuWar_DocN&#34;&#62;);&#13;&#10;&#13;&#10;  ser := '';&#13;&#10;  num := '';&#13;&#10;&#13;&#10;  if docSerNum &#60;&#62; '' then&#13;&#10;    begin&#13;&#10;      p :=  pos(' ', docSerNum);&#13;&#10;&#13;&#10;      if pos(' ', docSerNum) &#62; 0 then&#13;&#10;        begin&#13;&#10;          ser := copy(docSerNum, 1 , pos(' ', docSerNum) -1 );&#13;&#10;          num := copy(docSerNum,  pos(' ', docSerNum) + 1 , Length(docSerNum) -   pos(' ', docSerNum) + 1);&#13;&#10;        end&#13;&#10;      else&#13;&#10;        num := docSerNum;&#13;&#10;    end;&#13;&#10;&#13;&#10;   result := iif(isSer = true, ser , num);&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;function GetWarDt(type : integer = 0 {0-Day, 1-Month, 2-Year} ):string;&#13;&#10;var sDay, sMnth, sYear : string;&#13;&#10;begin&#13;&#10;  sDay := '';&#13;&#10;  sMnth := '';&#13;&#10;  sYear := '';&#13;&#10;&#13;&#10;  if YearOf(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;) &#62; 1900 then&#13;&#10;    begin&#13;&#10;      sDay := VarToStr(DayOf(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;));&#13;&#10;      sMnth := BPRO.FMONTH_NM_PAD(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;,0,1);&#13;&#10;      sYear := Copy(VarToStr(YearOf(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;)),1,2) + Copy(VarToStr(YearOf(&#60;qSoldier.&#34;KpuWar_DtDoc&#34;&#62;)),3,2);&#13;&#10;    end;&#13;&#10;&#13;&#10;  if type = 0 then&#13;&#10;    result := sDay&#13;&#10;  else if type = 1 then&#13;&#10;    result := sMnth&#13;&#10;  else&#13;&#10;    result := sYear;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Memo70OnBeforePrint(Sender: TfrxComponent);&#13;&#10;&#13;&#10;begin&#13;&#10;  //Memo70.text:=GetWarDocText();&#13;&#10;  Memo70.text :=  GetWarDocSerOrNum(true {Get Ser});&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo7.Text := GetWarDocSerOrNum(false {Get Num});&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; Memo9.Text := GetWarDt(0 {Get Day});&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; Memo10.Text := GetWarDt(1 {Get Month})&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo12OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; Memo12.Text := GetWarDt(2 {Get Year})&#13;&#10;end;&#13;&#10;&#13;&#10;function AddStrIfExist(checkText, addStr: string): string;&#13;&#10;begin&#13;&#10;  Result:= iif(trim(checkText)&#60;&#62;'', format('%s %s',[addStr,checkText]),'');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var docSerNum : string;&#13;&#10;begin&#13;&#10; { if (trim(mDocNm.Lines.Text)&#60;&#62;'') then&#13;&#10;    ReasonText:=Format('%s %s %s' ,[mDocNm.Lines.Text,AddStrIfExist(edDocCd.Text,'№'),AddStrIfExist(FormatDateTime(frm_date,dDocDate.Date),'від')])&#13;&#10;  else&#13;&#10;    begin&#13;&#10;       ReasonText:=Format('%s %s' ,[AddStrIfExist(&#60;qSoldier.&#34;KpuPrkz_Cd&#34;&#62;,'наказ №'),AddStrIfExist(iif(&#60;qSoldier.&#34;KpuPrkz_Dt&#34;&#62;=0,'',FormatDateTime(frm_date,&#60;qSoldier.&#34;KpuPrkz_Dt&#34;&#62;)),'від')]);&#13;&#10;    end;  }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PreparePodpis();&#13;&#10;var str_pp_sql:string;&#13;&#10;begin&#13;&#10;   if (GetCargoVal('BpI_MultiFirm') = 1)  and ( GetCargoVal('BpI_CdSte') &#60;&#62; 0) then&#13;&#10;   begin&#13;&#10;     str_pp_sql := 'INNER JOIN LISTSTE on LISTSTE_STATION = ' + vartostr(GetStationCd()) + ' AND  LISTSTE_CDSTE &#60;&#62; 0 AND ListSte_CdSte =  kdrpps.KdrPps_SE';&#13;&#10;     xstr_ReplaceSqlParam(PODPIS,'/*MultiFirm*/',str_pp_sql);&#13;&#10;   end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MainOnCloseQuery(Sender: TfrxComponent; var CanClose: Boolean);&#13;&#10;begin&#13;&#10;  if Main.ModalResult = 2 then StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure mChangeTextOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  _CheckStayOK();&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (RadioButton1.Checked) then RadioButton4.Checked := true&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if (RadioButton2.Checked) then RadioButton5.Checked := true&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton3OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if (RadioButton3.Checked) then RadioButton6.Checked := true&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo69OnBeforePrint(Sender: TfrxComponent);&#13;&#10;&#13;&#10;begin&#13;&#10;  PRK.Close;&#13;&#10;  PRK.ParamByName('rcd').Value := &#60;qSoldier.&#34;Kpu_Rcd&#34;&#62;;&#13;&#10;  PRK.Open;&#13;&#10;  osn := '';&#13;&#10;  if (RadioButton4.Checked) then&#13;&#10;    begin&#13;&#10;      osn := osn + RadioButton4.Caption;&#13;&#10;      osn := osn + ' № ';&#13;&#10;      osn := osn + IIF(edDocCd.Text&#60;&#62;'',edDocCd.Text,&#60;PRK.&#34;LAST_CD&#34;&#62;);&#13;&#10;      osn := osn + ' від ';&#13;&#10;      osn := osn + IIF(edDocCd.Text&#60;&#62;'',BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(dDocDate.Date, 7),'/','.',2,1),&#13;&#10;      BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(DateToStr(&#60;PRK.&#34;LAST_DTV&#34;&#62;), 7),'/','.',2,1));&#13;&#10;    end;&#13;&#10;  if (RadioButton5.Checked) then&#13;&#10;    begin&#13;&#10;      osn := osn + RadioButton5.Caption;&#13;&#10;      osn := osn + ' № ';&#13;&#10;      osn := osn + IIF(edDocCd.Text&#60;&#62;'',edDocCd.Text,&#60;PRK.&#34;MIN_CD&#34;&#62;);&#13;&#10;      osn := osn + ' від ';&#13;&#10;      osn := osn + IIF(edDocCd.Text&#60;&#62;'',BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(dDocDate.Date, 7),'/','.',2,1),&#13;&#10;      BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(DateToStr(&#60;PRK.&#34;MIN_DTV&#34;&#62;), 7),'/','.',2,1));&#13;&#10;    end;&#13;&#10;  if (RadioButton6.Checked) then&#13;&#10;    begin&#13;&#10;      osn := osn + RadioButton6.Caption;&#13;&#10;      osn := osn + ' № ';&#13;&#10;      osn := osn + IIF(edDocCd.Text&#60;&#62;'',edDocCd.Text,&#60;PRK.&#34;PER_CD&#34;&#62;);&#13;&#10;      osn := osn + ' від ';&#13;&#10;      if (edDocCd.Text&#60;&#62;'') then&#13;&#10;               osn := osn + BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(dDocDate.Date, 7),'/','.',2,1)&#13;&#10;      else if (YearOf(&#60;PRK.&#34;PER_DTV&#34;&#62;)&#62;1900) then&#13;&#10;               osn := osn + BPRO.PUTSTR_TO_STR(BPRO.FDATE_STR(DateToStr(&#60;PRK.&#34;PER_DTV&#34;&#62;), 7),'/','.',2,1);&#13;&#10;    end;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  CheckPlaceRun();&#13;&#10;  BPRO.SET_LANGUAGE('UA');&#13;&#10;end.">
