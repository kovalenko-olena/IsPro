    <TfrxODBCQuery Name="SatAvtNQuery" UserName="SatAvtNQuery" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="SELECT&#13;&#10;AVT_RCD&#13;&#10;,AVT_GARNMR&#13;&#10;,AVT_GAINMR&#13;&#10;,SG_NM&#13;&#10;,SG_TYPE&#13;&#10;,SPR_NM&#13;&#10;&#13;&#10;FROM SATAVTN&#13;&#10;&#13;&#10;LEFT JOIN SATGRPN&#13;&#10;ON ( SatAvtN.Avt_MrkRcd = SatGrpN.SG_Rcd)&#13;&#10;&#13;&#10;LEFT JOIN PSPR&#13;&#10;ON ( SatGrpN.SG_Type = PSpr.Spr_Cd) AND (PSpr.SprSpr_Cd=517)" PreprocessSQLText="true" SQLZeroCols="false" PropData="05704C656674023C0470546F7002140A506172616D65746572730100"/>
    <TfrxODBCQuery Name="Q" UserName="Q" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="--не отображается в отчете авто у которых пробег &#13;&#10;--и пробег спецоборудования за 2 мес = 0&#13;&#10;--т.к. будет деление на ноль&#13;&#10;--среднедневной пробег = 0&#13;&#10;--если пробег после то превышает нормы то, то надо выводить в отчет&#13;&#10;--независимо от среднесуточного пробега!(даже если 0)&#13;&#10;&#13;&#10;   select&#13;&#10;   AVT_RCD,&#13;&#10;   AVT_CRDNMR,&#13;&#10;   AVT_MRKRCD,&#13;&#10;   AVT_OSRCD,&#13;&#10;   AVT_DAT,&#13;&#10;   AVT_DATSPIS,&#13;&#10;   AVT_GAINMR,&#13;&#10;   AVT_GARNMR,&#13;&#10;   AVT_INVNMR,&#13;&#10;   SG_RCD,&#13;&#10;   SG_CD,&#13;&#10;   SG_NM,&#13;&#10;   (select SPR_NM from PSPR where (SG_Type = PSpr.Spr_Cd) AND (PSpr.SprSpr_Cd=517)) tip,  &#13;&#10;   SG_VTSCD,&#13;&#10;   SG_TYPE,&#13;&#10;   TYPE,           &#13;&#10;   SG_PRBTO1,&#13;&#10;   SG_PRBTO2,&#13;&#10;   SG_PRBTO3,  &#13;&#10;   AVT_APRCD,  &#13;&#10;   AVT_COLCD,  &#13;&#10;   AVT_GARCD,  &#13;&#10;   AVT_BRGCD,  &#13;&#10;   AVT_MKYEAR,  &#13;&#10;   SG_WGTALL,  &#13;&#10;   SATPRSTCMPRB,  &#13;&#10;   SATPRSTPMPRB,  &#13;&#10;   SATPHNTFIO,  &#13;&#10;   SATPHNTCD,  &#13;&#10;   SATPHNTTN,  &#13;&#10;   SMAZKA,  &#13;&#10;   TO1,  &#13;&#10;   TO2,  &#13;&#10;   PROB,  &#13;&#10;   OBOR,  &#13;&#10;   DAYS_IN,  &#13;&#10;   PROB4,  &#13;&#10;   PROB12,  &#13;&#10;   LASTTO1,  &#13;&#10;   LASTTO2,  &#13;&#10;   FAKT_PRB_TO1,  &#13;&#10;   FAKT_PRB_TO2,  &#13;&#10;   F_PRB_SPETS_TO1,  &#13;&#10;   F_PRB_SPETS_TO2,  &#13;&#10;   (--плановая дата ТО1 = дата последнего ТО, смещенная на количество дней, равное&#13;&#10;                --(максимальный пробег - фактический пробег) / среднесуточный пробег за 2 месяца&#13;&#10;                --DATEADD смещает на количество дней DAY стартовую дату MAX(ATPRPR_DATE)&#13;&#10;                SELECT &#13;&#10;                case when (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                          and PROB4&#60;&#62;0&#13;&#10;                          and TO1/((PROB+OBOR)/DAYS_IN)&#60;366*2&#13;&#10;                          and (SG_PRBTO1-isnull(isnull(FAKT_PRB_TO1, FAKT_PRB_TO2),0))&#62;0&#13;&#10;                          then&#13;&#10;                              DATEADD(DAY, (SG_PRBTO1-isnull(isnull(FAKT_PRB_TO1, FAKT_PRB_TO2),0))/((PROB+OBOR)/DAYS_IN ), :dat)&#13;&#10;                                         &#13;&#10;                          else &#13;&#10;                              case when (SG_PRBTO1-isnull(isnull(FAKT_PRB_TO1, FAKT_PRB_TO2),0))&#60;0 and &#13;&#10;                                        (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                                         and PROB4&#60;&#62;0&#13;&#10;                                         and TO1/((PROB+OBOR)/DAYS_IN)&#60;366*2&#13;&#10;                                         then DATEADD(DAY, 0, :dat)&#13;&#10;                              else&#13;&#10;                                   case when (SG_PRBTO1-isnull(isnull(FAKT_PRB_TO1, FAKT_PRB_TO2),0))&#62;0 and &#13;&#10;                                        (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                                         and PROB4&#60;&#62;0&#13;&#10;                                         and TO1/((PROB+OBOR)/DAYS_IN)&#62;366*2&#13;&#10;                                         then DATEADD(DAY, 366*2, :dat)&#13;&#10;                                         else&#13;&#10;                                           case when (SG_PRBTO1-isnull(isnull(FAKT_PRB_TO1, FAKT_PRB_TO2),0))&#60;0 and &#13;&#10;                                           PROB4&#60;&#62;0 &#13;&#10;                                           then DATEADD(DAY, 0, :dat)&#13;&#10;                                           end&#13;&#10;                                   end            &#13;&#10;                              end &#13;&#10;                          end &#13;&#10;                                                &#13;&#10;    ) plan_to1,     &#13;&#10;   (--плановая дата ТО1 = дата последнего ТО, смещенная на количество дней, равное&#13;&#10;                --(максимальный пробег - фактический пробег) / среднесуточный пробег за 2 месяца&#13;&#10;                --DATEADD смещает на количество дней DAY стартовую дату MAX(ATPRPR_DATE)&#13;&#10;                SELECT &#13;&#10;                case when (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                          and PROB4&#60;&#62;0&#13;&#10;                          and TO2/((PROB+OBOR)/DAYS_IN)&#60;366*2&#13;&#10;                          and (SG_PRBTO2-isnull(FAKT_PRB_TO2, 0))&#62;0&#13;&#10;                          then&#13;&#10;                              DATEADD(DAY, (SG_PRBTO2-isnull(FAKT_PRB_TO2, 0))/((PROB+OBOR)/DAYS_IN ), :dat)&#13;&#10;                                         &#13;&#10;                          else &#13;&#10;                              case when (SG_PRBTO2-isnull(FAKT_PRB_TO2, 0))&#60;0 and &#13;&#10;                                        (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                                         and PROB4&#60;&#62;0&#13;&#10;                                         and TO2/((PROB+OBOR)/DAYS_IN)&#60;366*2&#13;&#10;                                         then DATEADD(DAY, 0, :dat)&#13;&#10;                              else&#13;&#10;                                   case when (SG_PRBTO2-isnull(FAKT_PRB_TO2, 0))&#62;0 and &#13;&#10;                                        (PROB+OBOR)/DAYS_IN &#60;&#62;0 &#13;&#10;                                         and PROB4&#60;&#62;0&#13;&#10;                                         and TO2/((PROB+OBOR)/DAYS_IN)&#62;366*2&#13;&#10;                                         then DATEADD(DAY, 366*2, :dat)&#13;&#10;                                         else&#13;&#10;                                           case when (SG_PRBTO2-isnull(FAKT_PRB_TO2, 0))&#60;0 and &#13;&#10;                                           PROB4&#60;&#62;0 &#13;&#10;                                           then DATEADD(DAY, 0, :dat)&#13;&#10;                                           end&#13;&#10;                                   end            &#13;&#10;                              end &#13;&#10;                          end &#13;&#10;                                                &#13;&#10;    ) plan_to2    &#13;&#10;&#13;&#10;from             &#13;&#10;(--основной запрос                                                 &#13;&#10;SELECT&#13;&#10;      UF_RkValN as TYPE,    &#13;&#10;      AVT_RCD        AVT_RCD, &#13;&#10;      AVT_CRDNMR     AVT_CRDNMR, &#13;&#10;      AVT_MRKRCD     AVT_MRKRCD, &#13;&#10;      AVT_OSRCD      AVT_OSRCD, &#13;&#10;      AVT_DAT        AVT_DAT, &#13;&#10;      CASE WHEN AVT_OSRCD =0 THEN AVT_DATSPIS ELSE OS_VUBDAT END      AVT_DATSPIS, &#13;&#10;      AVT_GAINMR     AVT_GAINMR, &#13;&#10;      AVT_GARNMR     AVT_GARNMR, &#13;&#10;      AVT_INVNMR     AVT_INVNMR, &#13;&#10;      SG_RCD         SG_RCD, &#13;&#10;      SG_CD          SG_CD, &#13;&#10;      SG_NM          SG_NM, &#13;&#10;      SG_VTSCD       SG_VTSCD, &#13;&#10;      SG_TYPE        SG_TYPE, &#13;&#10;      SG_PRBTO1      SG_PRBTO1, &#13;&#10;      SG_PRBTO2      SG_PRBTO2, &#13;&#10;      SG_PRBTO3      SG_PRBTO3, &#13;&#10;      AVT_APRCD      AVT_APRCD, &#13;&#10;      AVT_COLCD      AVT_COLCD, &#13;&#10;      AVT_GARCD      AVT_GARCD, &#13;&#10;      AVT_BRGCD      AVT_BRGCD, &#13;&#10;      AVT_MKYEAR     AVT_MKYEAR, &#13;&#10;      SG_WGTALL      SG_WGTALL, &#13;&#10;      SPRATPWRKCP.ATPWRK_PR      SATPRSTCMPRB, &#13;&#10;      SPRATPWRKPP.ATPWRK_PR      SATPRSTPMPRB, &#13;&#10;      SPRSPVDN.SPVD_FIO      SATPHNTFIO, &#13;&#10;      SPRSPVDN.SPVD_VDCD     SATPHNTCD, &#13;&#10;      SPRSPVDN.SPVD_TN       SATPHNTTN, &#13;&#10;      &#13;&#10;       ( --объем смазки двигателя из настройки&#13;&#10;        SELECT UFRK6.UF_RKVALN AS U_SMAZKA &#13;&#10;        FROM &#34;UFPRV&#34; UFRK6 &#13;&#10;        WHERE UFRK6.UF_TBLID =1696 &#13;&#10;        AND UFRK6.UF_RKRCD=96 &#13;&#10;        AND UFRK6.UF_TBLRCD =SG_RCD &#13;&#10;        ) SMAZKA, --объем смазки двигателя из настройки&#13;&#10;      &#13;&#10;        (SG_PRBTO1) TO1, --максимальный пробег из настройки для проведения ТО1&#13;&#10;        (SG_PRBTO2) TO2, --максимальный пробег из настройки для проведения ТО2 &#13;&#10;&#13;&#10;        ( --пробег авто за 2 последних месяца&#13;&#10;         SELECT SUM(PP.ATPWRK_PR) &#13;&#10;         FROM ATPWRK PP &#13;&#10;         WHERE PP.ATPWRK_RCDAVT =AVT_RCD &#13;&#10;              AND &#13;&#10;              ((YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -1 ,:dat)) AND &#13;&#10;                MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -1 ,:dat)) ) OR &#13;&#10;               (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -2 ,:dat)) AND &#13;&#10;                MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -2 ,:dat)) ) ) &#13;&#10;         ) PROB, --пробег авто за 2 последних месяца&#13;&#10;       &#13;&#10;        (--пробег оборудования за 2 последних месяца&#13;&#10;         SELECT case when SG_Flg=1 then SUM(PP.ATPWRK_EQVTIM) else SUM(PP.ATPWRK_EQVTIM*25) end                                    &#13;&#10;         FROM ATPWRK PP&#13;&#10;         WHERE PP.ATPWRK_RCDAVT =AVT_RCD &#13;&#10;               AND &#13;&#10;               ((YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -1 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -1 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -2 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -2 ,:dat)) ) ) &#13;&#10;         ) OBOR, --пробег оборудования за 2 последних месяца&#13;&#10;         &#13;&#10;         (--количество кал.дней в периоде за 2 месяца до 01 числа отчетного мес.&#13;&#10;          SELECT DATEDIFF(DAY, DATEADD(MONTH, -2 , DATEADD(DAY,1-DAY(:dat), :dat)), &#13;&#10;          DATEADD(DAY,1-DAY(:dat), :dat))&#13;&#10;          ) DAYS_IN, --количество кал.дней в периоде за 2 месяца до 01 числа отчетного мес.&#13;&#10;          &#13;&#10;         (--пробег авто за 4 предшествующих месяца&#13;&#10;          (SELECT SUM(PP.ATPWRK_PR)&#13;&#10;          FROM ATPWRK PP &#13;&#10;          WHERE PP.ATPWRK_RCDAVT =AVT_RCD &#13;&#10;                AND &#13;&#10;                ((YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -1 ,:dat)) AND &#13;&#10;                  MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -1 ,:dat)) ) OR &#13;&#10;                 (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -2 ,:dat)) AND &#13;&#10;                  MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -2 ,:dat)) ) OR &#13;&#10;                 (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -3 ,:dat)) AND &#13;&#10;                  MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -3 ,:dat)) ) OR &#13;&#10;                 (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -4 ,:dat)) AND &#13;&#10;                  MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -4 ,:dat)) ) &#13;&#10;                 ) &#13;&#10;           ) +     &#13;&#10;         (SELECT  case when SG_Flg=1 then SUM(isnull(MM.PLST_EQVTIM,0)) else SUM(isnull(MM.PLST_EQVTIM,0))*25 end                          &#13;&#10;                FROM&#13;&#10;                   (SELECT TT.PLST_RCD, TT.PLST_EQVTIM &#13;&#10;                    FROM &#13;&#10;                        (SELECT PLST_RCD, PLST_EQVTIM &#13;&#10;                         FROM ATPPLPRB &#13;&#10;                         LEFT JOIN  PLST ON PLPRB_PLRCD=PLST_RCD   and plst_stt=1   &#13;&#10;                         WHERE &#13;&#10;                         PLST_AVTRCD=AVT_RCD &#13;&#10;                         AND PLPRB_DAT&#62;= DATEADD(MONTH, -4 ,:dat)&#13;&#10;                         AND PLPRB_DAT&#60; :dat &#13;&#10;                         )TT &#13;&#10;                     GROUP BY PLST_RCD, PLST_EQVTIM &#13;&#10;                    )MM                 &#13;&#10;           )      &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;                 &#13;&#10;           ) PROB4,--пробег авто за 4 предшествующих месяца&#13;&#10;    &#13;&#10;    &#13;&#10;         (--пробег авто за 12 предшествующих месяца&#13;&#10;          SELECT SUM(PP.ATPWRK_PR) &#13;&#10;          FROM ATPWRK PP &#13;&#10;          WHERE PP.ATPWRK_RCDAVT =AVT_RCD &#13;&#10;                AND &#13;&#10;               ((YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -1 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -1 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -2 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -2 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -3 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -3 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -4 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -4 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -5 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -5 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -6 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -6 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -7 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -7 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -8 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -8 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -9 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -9 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -10 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -10 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -11 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -11 ,:dat)) ) OR &#13;&#10;                (YEAR(PP.ATPWRK_DTPER) =YEAR(DATEADD(MONTH, -12 ,:dat)) AND &#13;&#10;                 MONTH(PP.ATPWRK_DTPER) =MONTH(DATEADD(MONTH, -12 ,:dat)) ) &#13;&#10;                ) &#13;&#10;               ) PROB12, --пробег авто за 12 предшествующих месяца&#13;&#10;               &#13;&#10;               &#13;&#10;             (--дата проведения последнего ТО1&#13;&#10;              select case when (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD)&#60;&#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) then &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) else &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD) end&#13;&#10;                 &#13;&#10;              ) LASTTO1, --дата проведения последнего ТО1&#13;&#10;              &#13;&#10;             (--дата проведения последнего ТО2&#13;&#10;              SELECT MAX(ATPRPR_DATE) &#13;&#10;              FROM  ATPRPR &#13;&#10;              WHERE &#13;&#10;              ATPRPR_TYP=3 &#13;&#10;              AND ATPRPR_RCDAVT=AVT_RCD&#13;&#10;              ) LASTTO2, --дата проведения последнего ТО2&#13;&#10;              &#13;&#10;              &#13;&#10;             (--факт. пробег авто от даты последнего ТО1 до 01 числа мес.отчета&#13;&#10;              SELECT SUM(isnull(PLPRB_PRB,0)) &#13;&#10;              FROM ATPPLPRB &#13;&#10;              LEFT JOIN  PLST ON PLPRB_PLRCD=PLst_Rcd and plst_stt=1                                 &#13;&#10;              WHERE &#13;&#10;              PLst_AvtRcd=AVT_RCD &#13;&#10;              AND PLPRB_DAT&#62;= &#13;&#10;                  (select case when (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD)&#60;&#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) then &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) else &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD) end) &#13;&#10;              AND PLPRB_DAT&#60;( :dat /*select CAST(CAST(YEAR(dat) AS VARCHAR(4))+'-'+ &#13;&#10;                            CAST(MONTH(dat) AS VARCHAR(2))+'-'+ &#13;&#10;                            CAST('01' AS VARCHAR(2)) AS DATETIME)*/) &#13;&#10;              ) FAKT_PRB_TO1, --факт. пробег авто от даты последнего ТО1 до 01 числа мес.отчета&#13;&#10;              &#13;&#10;              &#13;&#10;              (--факт. пробег авто от даты последнего ТО2 до 01 числа мес.отчета&#13;&#10;               SELECT SUM(isnull(PLPRB_PRB,0)) &#13;&#10;               FROM ATPPLPRB &#13;&#10;               LEFT JOIN  PLST ON PLPRB_PLRCD=PLST_RCD  and plst_stt=1      &#13;&#10;               WHERE PLST_AVTRCD=AVT_RCD &#13;&#10;               AND PLPRB_DAT&#62;= &#13;&#10;                   (SELECT MAX(ATPRPR_DATE) &#13;&#10;                    FROM  ATPRPR &#13;&#10;                    WHERE &#13;&#10;                    ATPRPR_TYP=3 &#13;&#10;                    AND ATPRPR_RCDAVT=AVT_RCD) &#13;&#10;                    &#13;&#10;               AND PLPRB_DAT&#60; :dat /*CAST(CAST(YEAR(dat) AS VARCHAR(4))+'-'+ &#13;&#10;                    CAST(MONTH(dat) AS VARCHAR(2))+'-'+ &#13;&#10;                    CAST('07' AS VARCHAR(2)) AS DATETIME)*/ &#13;&#10;               ) FAKT_PRB_TO2, --факт. пробег авто от даты последнего ТО2 до 01 числа мес.отчета&#13;&#10;               &#13;&#10;               &#13;&#10;               (--факт. пробег спецоборудования от даты последнего ТО1 до 01 числа мес.отчета&#13;&#10;                SELECT  case when SG_Flg=1 then SUM(isnull(MM.PLST_EQVTIM,0)) else SUM(isnull(MM.PLST_EQVTIM,0))*25 end                          &#13;&#10;                FROM&#13;&#10;                   (SELECT TT.PLST_RCD, TT.PLST_EQVTIM &#13;&#10;                    FROM &#13;&#10;                        (SELECT PLST_RCD, PLST_EQVTIM &#13;&#10;                         FROM ATPPLPRB &#13;&#10;                         LEFT JOIN  PLST ON PLPRB_PLRCD=PLST_RCD  and plst_stt=1      &#13;&#10;                         WHERE &#13;&#10;                         PLST_AVTRCD=AVT_RCD &#13;&#10;                         AND PLPRB_DAT&#62;= &#13;&#10;                            (select case when (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD)&#60;&#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) then &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR  WHERE ATPRPR_TYP=3 AND ATPRPR_RCDAVT=AVT_RCD) else &#13;&#10;              (SELECT MAX(ATPRPR_DATE) FROM  ATPRPR WHERE ATPRPR_TYP=2 AND ATPRPR_RCDAVT=AVT_RCD) end) &#13;&#10;                         AND PLPRB_DAT&#60; :dat /*CAST(CAST(YEAR(dat) AS VARCHAR(4))+'-'+ &#13;&#10;                             CAST(MONTH(dat) AS VARCHAR(2))+'-'+ &#13;&#10;                             CAST('07' AS VARCHAR(2)) AS DATETIME) */&#13;&#10;                         )TT &#13;&#10;                     GROUP BY PLST_RCD, PLST_EQVTIM &#13;&#10;                    )MM&#13;&#10;                 ) F_PRB_SPETS_TO1,--факт. пробег спецоборудования от даты последнего ТО1 до 01 числа мес.отчета&#13;&#10;                    &#13;&#10;                    &#13;&#10;               (--факт. пробег спецоборудования от даты последнего ТО2 до 01 числа мес.отчета&#13;&#10;                SELECT case when SG_Flg=1 then SUM(isnull(MM.PLST_EQVTIM,0)) else  SUM(isnull(MM.PLST_EQVTIM,0))*25 end                            &#13;&#10;                FROM&#13;&#10;                    (SELECT TT.PLST_RCD, TT.PLST_EQVTIM &#13;&#10;                     FROM &#13;&#10;                         (SELECT PLST_RCD, PLST_EQVTIM &#13;&#10;                          FROM ATPPLPRB &#13;&#10;                          LEFT JOIN  PLST ON PLPRB_PLRCD=PLST_RCD  and plst_stt=1   &#13;&#10;                          WHERE &#13;&#10;                          PLST_AVTRCD=AVT_RCD &#13;&#10;                          AND PLPRB_DAT&#62;= &#13;&#10;                              (SELECT MAX(ATPRPR_DATE) &#13;&#10;                               FROM  ATPRPR &#13;&#10;                               WHERE &#13;&#10;                               ATPRPR_TYP=3 &#13;&#10;                               AND ATPRPR_RCDAVT=AVT_RCD&#13;&#10;                               ) &#13;&#10;                          AND PLPRB_DAT&#60; :dat /*CAST(CAST(YEAR(dat) AS VARCHAR(4))+'-'+ &#13;&#10;                                        CAST(MONTH(dat) AS VARCHAR(2))+'-'+ &#13;&#10;                                        CAST('07' AS VARCHAR(2)) AS DATETIME) */&#13;&#10;                          )TT &#13;&#10;                    GROUP BY PLST_RCD, PLST_EQVTIM &#13;&#10;                    )MM&#13;&#10;                ) F_PRB_SPETS_TO2 --факт. пробег спецоборудования от даты последнего ТО2 до 01 числа мес.отчета&#13;&#10;                &#13;&#10;                &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;--основной хвостик запроса стянутЫй не долго думая с вида реестра                                                                                                     &#13;&#10;FROM SATAVTN &#13;&#10;left join UFPRV   on AVT_MRKRCD=UF_TblRcd and UF_RkRcd=102    &#13;&#10;INNER JOIN SATGRPN ON AVT_MRKRCD =SG_RCD&#13;&#10;LEFT JOIN OSNKRT ON AVT_OSRCD =OS_RCD &#13;&#10;LEFT JOIN ATPWRK SPRATPWRKCP ON SPRATPWRKCP.ATPWRK_RCDAVT =AVT_RCD AND SPRATPWRKCP.ATPWRK_DTPER =:dat &#13;&#10;LEFT JOIN ATPWRK SPRATPWRKPP ON SPRATPWRKPP.ATPWRK_RCDAVT =AVT_RCD AND SPRATPWRKPP.ATPWRK_DTPER =:dat       &#13;&#10;LEFT JOIN SPVDN SPRSPVDN ON SPVD_RCD IN &#13;&#10;( &#13;&#10;SELECT TOP 1 ATPAVTVOD_VOD &#13;&#10;FROM ATPAVTVOD &#13;&#10;WHERE ATPAVTVOD.ATPAVTVOD_AVT =SATAVTN.AVT_RCD &#13;&#10;ORDER BY ATPAVTVOD.ATPAVTVOD_VOD &#13;&#10;) &#13;&#10; WHERE &#13;&#10;(0 =0 OR AVT_RCD =0) AND &#13;&#10;(0 =0 OR 0 &#62;0 AND &#13;&#10;(CASE WHEN AVT_OSRCD =0 THEN AVT_DATSPIS ELSE OS_VUBDAT END) ='18761231') AND &#13;&#10;(0 =0 OR  0 &#62;0 AND SG_RCD =0) AND &#13;&#10;(0   =0 OR 0  &#62;0 AND SG_VTSCD    =(0 - 1)) AND &#13;&#10;(0  =0 OR 0 &#62;0 AND SG_TYPE     =0) AND &#13;&#10;(0   =0 OR 0  &#62;0 AND AVT_APRCD   =0 ) AND &#13;&#10;(0   =0 OR 0  &#62;0 AND AVT_COLCD   =0 ) AND &#13;&#10;(0   =0 OR 0  &#62;0 AND AVT_GARCD   =0 ) AND &#13;&#10;(0   =0 OR 0  &#62;0 AND AVT_BRGCD   =0 ) AND &#13;&#10;(0  =0 OR 0 &#62;0 AND AVT_INVNMR &#13;&#10;&#62;= 0) AND &#13;&#10;(0  =0 OR 0 &#62;0 AND AVT_INVNMR &#13;&#10;&#60;= 0) AND &#13;&#10;(0  =0 OR 0 &#62;0 AND AVT_MKYEAR &#13;&#10;&#62;= 0) AND &#13;&#10;(0  =0 OR 0 &#62;0 AND AVT_MKYEAR &#13;&#10;&#60;= 0) AND &#13;&#10;(0.00  =0 OR 0.00 &#62;0 AND SG_WGTALL &#13;&#10;&#62;= 0.00) AND &#13;&#10;(0.00  =0 OR 0.00 &#62;0 AND SG_WGTALL &#13;&#10;&#60;= 0.00) &#13;&#10;&#13;&#10;&#13;&#10;)mm&#13;&#10;&#13;&#10;order by  &#13;&#10;  AVT_COLCD,    (select SPR_NM from PSPR where (SG_Type = PSpr.Spr_Cd) AND (PSpr.SprSpr_Cd=517)) ,    AVT_GAINMR                                                        " PreprocessSQLText="true" SQLZeroCols="false" PropData=""/>
