<TfrxReport Version="5.4.3" DotMatrixReport="false" IniFile="\Software\Fast Reports" OldStyleProgress="true" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Коваленко Е.Я. Mоскалёва Т.Г." ReportOptions.CreateDate="38904,4143833912" ReportOptions.Description.Text="750 FR Звіт про здійснення відрахувань та виплат" ReportOptions.Name="FR Звiт про здiйснення вiдрахувань та виплат (АЛIМ-ВIД.1)+вiйськовi 12.2022" ReportOptions.LastChange="44896,4703900579" ReportOptions.Language="rpgUA" ReportOptions.DirectExportType="deRtf" ReportOptions.DirectExportMode="dmOpen" ReportOptions.DirectExportPages="depSingleSheet" ReportOptions.DirectExportChunkSize="0" ScriptLanguage="PascalScript" ScriptText.Text="//===================================================================&#13;&#10;//  Отчет 750 Ведомость удержания алиментов&#13;&#10;//&#13;&#10;//===================================================================&#13;&#10;// 19.05.2021 Савицкая&#13;&#10;// Удержания для алиментов : +контрагент+ФИО получателя алиментов&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;var i,sml,lnn:integer;&#13;&#10;var i2,z,y:integer;&#13;&#10;var str_pd,str_vo,ss1,ss:string;&#13;&#10;var ValMt:integer;&#13;&#10;var DTUP1,LDTUP1,DTUP2,LDTUP2:TDateTime;&#13;&#10;var sm_u,sm_p,sm_z,sm_tax, sm_sld:double;&#13;&#10;var sm_bank,sm_sbbank,sm_kasa,sm_posht,sm_r:double;&#13;&#10;var datn,datk:TDateTime;&#13;&#10;var kolmon:Integer;&#13;&#10;var flg_Pacc,flg_Tacc,flg_SEAcc,flg_KtSAcc:integer;&#13;&#10;var CdFrm,pSubPdr:integer;&#13;&#10;var strPacc,strTacc,strSEAcc,strKtSAcc:string;&#13;&#10;var _Error: Boolean;&#13;&#10;&#13;&#10;// Определяет количество месяцев между двумя датами&#13;&#10;function KolMonth(dat1 :TDATETIME; dat2 :TDATETIME) :Integer;&#13;&#10;var m1,m2,y1,y2 :integer;&#13;&#10;begin&#13;&#10;  m1 := MonthOf(dat1);&#13;&#10;  m2 := MonthOf(dat2);&#13;&#10;  y1 := YearOf(dat1);&#13;&#10;  y2 := YearOf(dat2);&#13;&#10;  Result := (y2-y1)*12+(m2-m1);&#13;&#10;end;&#13;&#10;&#13;&#10;// ===== Для обработки TreeView&#13;&#10;var pRec_Ct:integer;&#13;&#10;var mspd:array[1..1]of string;&#13;&#10;var l:integer;&#13;&#10;var str_pcd,StrPdrSql,str_grppd:string;&#13;&#10;var RcdMrk,RcdMrkC:string;&#13;&#10;var RcdUncMrk:string;&#13;&#10;var Tmp_RcdMrk:string;&#13;&#10;var flg_Alt:integer;&#13;&#10;&#13;&#10;// Получить код родительского подразделения для группировки&#13;&#10;function GetGrpCd( pPD:String; pStr:String ):String;&#13;&#10;var tmpStr:String;&#13;&#10;var ResStr:String;&#13;&#10;begin&#13;&#10;  ResStr:='';&#13;&#10;  tmpStr:= pStr;&#13;&#10;  while Pos( ',', tmpStr )&#60;&#62;0 do&#13;&#10;    begin&#13;&#10;      If Copy( tmpStr, 1, Pos( ',', tmpStr )-1 )=Copy( pPD, 1, Length( Copy( tmpStr, 1, Pos( ',', tmpStr )-1 ) ) ) then&#13;&#10;        begin&#13;&#10;          ResStr:=Copy( tmpStr, 1, Pos( ',', tmpStr )-1 );&#13;&#10;//          break;    05.01.2007 wolf&#13;&#10;        end;&#13;&#10;      Delete( tmpStr, 1, Pos( ',', tmpStr ) );&#13;&#10;    end;&#13;&#10;   If tmpStr=Copy( pPD, 1, Length( tmpStr ) ) then&#13;&#10;     begin&#13;&#10;       ResStr:=Copy( tmpStr, 1, Length( tmpStr ) );&#13;&#10;//       break;       05.01.2007 wolf&#13;&#10;     end;&#13;&#10;   Delete( tmpStr, 1, Length( tmpStr ) );&#13;&#10;   Result:= ResStr;&#13;&#10;end;&#13;&#10;&#13;&#10;function DT_STR(D:TDateTime):string;  //(dd.mm.yyyy)&#13;&#10;var dd,mm:string;&#13;&#10;begin&#13;&#10;  If Length(DayOf(D))=1 then&#13;&#10;    begin&#13;&#10;      dd:='0'+IntToStr(DayOf(D));&#13;&#10;    end&#13;&#10;  Else If Length(DayOf(D))=2 then&#13;&#10;    begin&#13;&#10;      dd:=IntToStr(DayOf(D));&#13;&#10;    end;&#13;&#10;  If Length(MonthOf(D))=1 then&#13;&#10;    begin&#13;&#10;      mm:='0'+IntToStr(MonthOf(D));&#13;&#10;    end&#13;&#10;  Else If Length(MonthOf(D))=2 then&#13;&#10;    begin&#13;&#10;      mm:=IntToStr(MonthOf(D));&#13;&#10;    end;&#13;&#10;  If YearOf(D)&#60;1900 then&#13;&#10;    begin&#13;&#10;      Result:='';&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      Result:=dd+'.'+mm+'.'+IntToStr(YearOf(D));&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  CheckBox2.Checked:=false;&#13;&#10;&#13;&#10;  ComboBox1.Items.Clear;&#13;&#10;  ComboBox2.Items.Clear;&#13;&#10;&#13;&#10;  If (GetUserRepParam('CB1A')='') OR (GetUserRepParam('CB1A')='0') then&#13;&#10;    begin&#13;&#10;      pSubPdr := 0;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      pSubPdr := 1;&#13;&#10;    end;&#13;&#10;  If (GetUserRepParam('RB1A')='') OR (GetUserRepParam('RB1A')='0') then&#13;&#10;    begin&#13;&#10;      RadioButton1.Checked := True;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      RadioButton2.Checked := True;&#13;&#10;    end;&#13;&#10;&#13;&#10;  RadioButton3.Checked := True;&#13;&#10;  Page1.Visible:=True;&#13;&#10;  Page4.Visible:=False;&#13;&#10;&#13;&#10;  If (GetUserRepParam('СB2A')='1') then&#13;&#10;     CheckBox1.Checked := True&#13;&#10;  else&#13;&#10;     CheckBox1.Checked := False;&#13;&#10;&#13;&#10;  qDat.Open;&#13;&#10;  datn := &#60;qDat.&#34;datn&#34;&#62;;&#13;&#10;  datk := BPRO.CUR_UP().Value;&#13;&#10;  kolmon := KolMonth(datn,datk);&#13;&#10;  datk := datn;&#13;&#10;&#13;&#10;  for i := 0 to kolmon do&#13;&#10;    begin&#13;&#10;      If datk = BPRO.CUR_UP().Value then&#13;&#10;        begin&#13;&#10;          combobox1.Items.add(' '+BPRO.FMONTH_NAME(datk,1)+' '+chr(39)+Copy(inttostr(YearOf(datk)),3,2)+' (текущий)');&#13;&#10;          combobox2.Items.add(' '+BPRO.FMONTH_NAME(datk,1)+' '+chr(39)+Copy(inttostr(YearOf(datk)),3,2)+' (текущий)');&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          combobox1.Items.add(' '+BPRO.FMONTH_NAME(datk,1)+' '+chr(39)+Copy(inttostr(YearOf(datk)),3,2)+' (закрытый)');&#13;&#10;          combobox2.Items.add(' '+BPRO.FMONTH_NAME(datk,1)+' '+chr(39)+Copy(inttostr(YearOf(datk)),3,2)+' (закрытый)');&#13;&#10;        end;&#13;&#10;      datk := IncMonth(datk,1);&#13;&#10;    end;&#13;&#10;&#13;&#10;  If GetUserRepParam('DTnA') = '' then&#13;&#10;    begin&#13;&#10;      Combobox1.ItemIndex := kolmon;&#13;&#10;      SetUserRepParam('DTNA',IntToStr(ComboBox1.ItemIndex));&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      If StrToInt(GetUserRepParam('DTnA')) &#62; kolmon then&#13;&#10;        begin&#13;&#10;          Combobox1.ItemIndex := kolmon;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          Combobox1.ItemIndex := StrToInt(GetUserRepParam('DTnA'));&#13;&#10;        end;&#13;&#10;    end;&#13;&#10;&#13;&#10;  If GetUserRepParam('DTkA') = '' then&#13;&#10;    begin&#13;&#10;      Combobox2.ItemIndex := kolmon;&#13;&#10;      SetUserRepParam('DTKA',IntToStr(ComboBox2.ItemIndex));&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      If StrToInt(GetUserRepParam('DTkA')) &#62; kolmon then&#13;&#10;        begin&#13;&#10;          Combobox2.ItemIndex := kolmon;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          Combobox2.ItemIndex := StrToInt(GetUserRepParam('DTkA'));&#13;&#10;        end;&#13;&#10;    end;&#13;&#10;&#13;&#10;  If RadioButton1.Checked=True then&#13;&#10;    begin&#13;&#10;      Button2.Enabled:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      Button2.Enabled:=True;&#13;&#10;    end;&#13;&#10;  Edit1.Text := GetComonRepParam('E1A');&#13;&#10;  Edit2.Text := GetComonRepParam('E2A');&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button2OnClick(Sender: TfrxComponent);&#13;&#10;var formPD:integer;&#13;&#10;begin&#13;&#10;  LDTUP1 := EncodeDate(YearOf(IncMonth(datn,Combobox1.ItemIndex)),MonthOf(IncMonth(datn,Combobox1.ItemIndex)),DaysInMonth(YearOf(IncMonth(datn,Combobox1.ItemIndex)),MonthOf(IncMonth(datn,Combobox1.ItemIndex))));&#13;&#10;  LDTUP2 := EncodeDate(YearOf(IncMonth(datn,Combobox2.ItemIndex)),MonthOf(IncMonth(datn,Combobox2.ItemIndex)),DaysInMonth(YearOf(IncMonth(datn,Combobox2.ItemIndex)),MonthOf(IncMonth(datn,Combobox2.ItemIndex))));&#13;&#10;  formPD := TfrxDialogPage(SPRPD).ShowModal;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SPRPDOnResize(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  CheckBox1.Anchors:=[akBottom,akLeft];&#13;&#10;  Bevel3.Anchors:=[akBottom,akRight,akLeft];&#13;&#10;  Bevel4.Anchors:=[akBottom,akRight,akLeft];&#13;&#10;  Button5.Anchors:=[akBottom,akRight];&#13;&#10;  Button1.Anchors:=[akBottom,akRight];&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button4OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnCloseQuery(Sender: TfrxComponent; var CanClose: Boolean);&#13;&#10;begin&#13;&#10;  If Page2.ModalResult=2 then&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button3OnClick(Sender: TfrxComponent);&#13;&#10;var str_sql:string;&#13;&#10;begin&#13;&#10;  SPRPD.Visible:=False;&#13;&#10;  Page3.Visible := False;&#13;&#10;  DTUP1  := EncodeDate(YearOf(IncMonth(datn,Combobox1.ItemIndex)),MonthOf(IncMonth(datn,Combobox1.ItemIndex)),1);&#13;&#10;  DTUP2  := EncodeDate(YearOf(IncMonth(datn,Combobox2.ItemIndex)),MonthOf(IncMonth(datn,Combobox2.ItemIndex)),1);&#13;&#10;  LDTUP1 := EncodeDate(YearOf(IncMonth(datn,Combobox1.ItemIndex)),MonthOf(IncMonth(datn,Combobox1.ItemIndex)),DaysInMonth(YearOf(IncMonth(datn,Combobox1.ItemIndex)),MonthOf(IncMonth(datn,Combobox1.ItemIndex))));&#13;&#10;  LDTUP2 := EncodeDate(YearOf(IncMonth(datn,Combobox2.ItemIndex)),MonthOf(IncMonth(datn,Combobox2.ItemIndex)),DaysInMonth(YearOf(IncMonth(datn,Combobox2.ItemIndex)),MonthOf(IncMonth(datn,Combobox2.ItemIndex))));&#13;&#10;&#13;&#10;  If RadioButton1.Checked then&#13;&#10;    begin&#13;&#10;      SetUserRepParam('RB1A','0');&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      SetUserRepParam('RB1A','1');&#13;&#10;    end;&#13;&#10;  if CheckBox1.Checked then&#13;&#10;    SetUserRepParam('CB2A','1')&#13;&#10;  else&#13;&#10;    SetUserRepParam('CB2A','0');&#13;&#10;&#13;&#10;    SetComonRepParam('E1A',Edit1.Text);&#13;&#10;    SetComonRepParam('E2A',Edit2.Text);&#13;&#10;&#13;&#10;&#13;&#10; ProcessMessages;&#13;&#10;  If DTUP1 = BPRO.CUR_UP().Value or DTUP2 = BPRO.CUR_UP().Value then&#13;&#10;    begin&#13;&#10;      If BPRO.PAY_CALC_PRED(1).Value = 0 THEN StopCloseReport;&#13;&#10;    end;&#13;&#10;&#13;&#10;  str_sql:=str_sql+' select KpuUdr1.KpuUdr_Rcd,              '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   KpuRlo1.KpuRl_CdVo,                   ' +Chr(13);&#13;&#10;  str_sql:=str_sql+'   payvo1.vo_cdchr as Vo_Cd,             '+Chr(13);&#13;&#10;  str_sql:=str_sql+'       (select top 1 KpuNch_DatN         '+Chr(13);&#13;&#10;  str_sql:=str_sql+'       from kpunch1                      '+Chr(13);&#13;&#10;  str_sql:=str_sql+'       where kpunch1.kpu_rcd=kpux.kpu_rcd'+Chr(13);&#13;&#10;  str_sql:=str_sql+'       and kpunch1.KpuNch_Cd=493         '+Chr(13);&#13;&#10;  str_sql:=str_sql+'       and kpunch1.KpuNch_DatN &#60;= :DT2 and (kpunch1.KpuNch_DatK &#62;= :DTn or YEAR(kpunch1.KpuNch_DatK)=1900)) as DatN,'+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpurlo1.kpu_tn,                       '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuc1.kpu_fio,                        '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   {fn convert(kpurlo1.kpurl_sm,SQL_DECIMAL)}/({fn Power(10,:mt)}) as kpurl_sm, '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpurlo1.kpurl_datup as datup,         '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   podr.sprpdr_pd,                       '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   podr.sprpdr_nmfull,                   '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_cdpr as isplst,        '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_DtPr as DtPr,          '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_sm,                    '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_mt,                    '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_datn,                  '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_datk,                  '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudrplc_fio,                '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_adr,                   '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_ind,                   '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_rs,                    '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudr_ls,                    '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpurlo1.kpurlpl_srz as ish_sm,        '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudrplc_typ,                '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   kpuudr1.kpuudrplc_pvgr as VED,        '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   ptnrk.ptn_nm as NMKAG,                '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   {fn MOD({fn TRUNCATE(kpurlo1.kpurl_prz/2,0)},2)} as prz, '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   CASE WHEN KpuUdr1.KpuUdr_Rest IS NULL THEN 0 ELSE {fn convert(KpuUdr1.KpuUdr_Rest,SQL_DECIMAL)}/({fn Power(10,:mt1)}) END AS KpuUdr_Rest,'+Chr(13);&#13;&#10;  str_sql:=str_sql+'   KpuRlo1.KpuRl_Rcd,                    '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   TAX,                                  '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   nar,                                  '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   TAXALL,                               '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   {fn convert(kpurlo1.kpurldu_sld,SQL_DECIMAL)}/({fn Power(10,:mt)}) as sld, '+Chr(13);&#13;&#10;   //***************назва виконавчого документу з постійних утримань&#13;&#10;  str_sql:=str_sql+'   KpuUdr_ExecDoc as Nazva               '+Chr(13);&#13;&#10;  str_sql:=str_sql+'                                         '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   from kpurlo1                          '+Chr(13);&#13;&#10;  str_sql:=str_sql+'   inner join kpux on kpurlo1.kpu_tn=kpux.kpu_tn'+Chr(13);&#13;&#10;  str_sql:=str_sql+'   inner join kpuc1 on kpux.kpu_rcd=kpuc1.kpu_rcd'+Chr(13);&#13;&#10;&#13;&#10;  // Доступ к структурным единицам&#13;&#10;  If flg_SEAcc = 1 then&#13;&#10;    begin&#13;&#10;      str_sql := str_sql+' INNER JOIN /*TABLEVALUED(*/fnKpuPdrByDate( :sSprPdrDate )/*)TABLEVALUED*/ FnPdrCd on KpuC1.Kpu_Rcd = FnPdrCd.Kpu_Rcd'+Chr(13);&#13;&#10;      str_sql := str_sql+' INNER JOIN PayAccPdrSE PdrSE ON FnPdrCd.KpuK_CdPd = PdrSE.PayAcc_Pdr AND FnPdrCd.SprPdr_Dt = PdrSE.PayAcc_PdrDt'+Chr(13);&#13;&#10;    end;&#13;&#10;  str_sql:=str_sql+' inner join kpuprk1 on kpuprk1.bookmark='+Chr(13);&#13;&#10;  str_sql:=str_sql+' ('+Chr(13);&#13;&#10;  str_sql:=str_sql+' select max(p1.bookmark)'+Chr(13);&#13;&#10;  str_sql:=str_sql+' from kpuprk1 p1'+Chr(13);&#13;&#10;  str_sql:=str_sql+' where p1.kpu_rcd=kpuc1.kpu_rcd'+Chr(13);&#13;&#10;  str_sql:=str_sql+' and p1.kpuprkz_dtv='+Chr(13);&#13;&#10;  str_sql:=str_sql+' ('+Chr(13);&#13;&#10;  str_sql:=str_sql+' select max(p2.kpuprkz_dtv)'+Chr(13);&#13;&#10;  str_sql:=str_sql+' from kpuprk1 p2'+Chr(13);&#13;&#10;  str_sql:=str_sql+' where p2.kpuprkz_dtv&#60;=:DT1'+Chr(13);&#13;&#10;  str_sql:=str_sql+' and p2.kpu_rcd=kpuc1.kpu_rcd'+Chr(13);&#13;&#10;  str_sql:=str_sql+' )'+Chr(13);&#13;&#10;  str_sql:=str_sql+' )'+Chr(13);&#13;&#10;  str_sql:=str_sql+' inner join payvo1 on kpurlo1.kpurl_cdvo=payvo1.vo_cd'+Chr(13);&#13;&#10;&#13;&#10;  str_sql:=str_sql+' left join '+Chr(13);&#13;&#10;  str_sql:=str_sql+'           (select sum ({fn convert(kpurl_sm,SQL_DECIMAL)})/{fn Power(10,:mt)} as nar, kpu_tn as tabn, kpurl_datup as datp'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            from kpurlo1 '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            inner join payvo1 on kpurl_cdvo=vo_cd '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            where (kpurl_datup&#62;=:DTn and kpurl_datup&#60;=:DTk) and vo_grp&#60;=127 and  vo_nur=0 '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            and kpurl_cdvo in (select paytv_cdv from paytv where paytv_part=1 and paytv_cd='+str_vo+' and paytv_nmr=33) '+Chr(13);&#13;&#10;&#13;&#10;  str_sql:=str_sql+'            group by kpu_tn, kpurl_datup '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            ) kk'+Chr(13);&#13;&#10;  str_sql:=str_sql+' on kpurlo1.kpu_tn=kk.tabn and kpurlo1.kpurl_datup=kk.datp '+Chr(13);&#13;&#10;  str_sql:=str_sql+' left join '+Chr(13);&#13;&#10;  str_sql:=str_sql+'           (select sum ({fn convert(kpurl_sm,SQL_DECIMAL)})/{fn Power(10,:mt)} as taxALL, kpu_tn as tabn, kpurl_datup as datp'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            from kpurlo1 '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            inner join payvo1 on kpurl_cdvo=vo_cd'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            where (kpurl_datup&#62;=:DTn and kpurl_datup&#60;=:DTk) and vo_grp=128 group by kpu_tn, kpurl_datup'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            ) pp'+Chr(13);&#13;&#10;  str_sql:=str_sql+' on kpurlo1.kpu_tn=pp.tabn and kpurlo1.kpurl_datup=pp.datp '+Chr(13);&#13;&#10;&#13;&#10;  str_sql:=str_sql+' left join '+Chr(13);&#13;&#10;  str_sql:=str_sql+'           (select sum ({fn convert(kpurl_sm,SQL_DECIMAL)})/{fn Power(10,:mt)} as TAX, kpu_tn as tabn, kpurl_datup as datp'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            from kpurlo1 '+Chr(13);&#13;&#10;  str_sql:=str_sql+'            inner join payvo1 on kpurl_cdvo=vo_cd'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            where (kpurl_datup&#62;=:DTn and kpurl_datup&#60;=:DTk) and vo_grp&#60;=127 and vo_nur=1 group by kpu_tn, kpurl_datup'+Chr(13);&#13;&#10;  str_sql:=str_sql+'            ) kkK'+Chr(13);&#13;&#10;  str_sql:=str_sql+' on kpurlo1.kpu_tn=kkK.tabn and kpurlo1.kpurl_datup=kkK.datp '+Chr(13);&#13;&#10;  str_sql:=str_sql+' left join sprpdr1 podr on kpuprk1.kpuprkz_pdRcd = podr.sprpdr_Rcd and :DT2 between podr.sprpdr_datN and podr.sprpdr_datK'+Chr(13);&#13;&#10;  str_sql:=str_sql+'