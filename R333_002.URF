<TfrxReport Version="5.4.3" DotMatrixReport="false" IniFile="\Software\Fast Reports" OldStyleProgress="true" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="39884,5099712384" ReportOptions.Description.Text="" ReportOptions.Name="FR ЮГОК Налаштування доступу управління персоналом" ReportOptions.LastChange="45034,7336832407" ReportOptions.DirectExportType="deRtf" ReportOptions.DirectExportMode="dmOpen" ReportOptions.DirectExportPages="depSingleSheet" ReportOptions.DirectExportChunkSize="0" ScriptLanguage="PascalScript" ScriptText.Text="// ====================================================================&#13;&#10;// Настройка доступа Управление персоналом&#13;&#10;//&#13;&#10;// ====================================================================&#13;&#10;Var&#13;&#10;  spdr_zp, spdr_kdr :string;&#13;&#10;  sse_zp, sse_kdr :string;&#13;&#10;  stn_zp, stn_kdr :string;&#13;&#10;  sjr_zp, sjr_kdr :string;&#13;&#10;  sset_zp, sset_kdr :string;&#13;&#10;  bpdr_zp, bpdr_kdr :boolean;&#13;&#10;  bse_zp, bse_kdr :boolean;&#13;&#10;  btn_zp, btn_kdr :boolean;&#13;&#10;  bjr_zp, bjr_kdr :boolean;&#13;&#10;  bset_zp, bset_kdr :boolean;&#13;&#10;&#13;&#10;  iSettings: integer;&#13;&#10;  {биты:&#13;&#10;  0 - общие параметры&#13;&#10;  1 - подразделения&#13;&#10;  2 - структурные единицы&#13;&#10;  3 - диапазон табельных номеров&#13;&#10;  4 - журналы&#13;&#10;  5 - настройки&#13;&#10;  6 - секретность&#13;&#10;  7 - документы}&#13;&#10;&#13;&#10;  LNG : byte;&#13;&#10;&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;//---------------- USER FUNC -------------------------------------------------------&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;// Caiaiyao no?ieo a no?iea&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;&#13;&#10;&#13;&#10;function RuUa (Ru: string; Ua :string;) :string;&#13;&#10;begin&#13;&#10;  if LNG = 1 then&#13;&#10;     Result := Ru&#13;&#10;  else&#13;&#10;     Result := Ua;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReplaceString ( var SourceString:String; OldPattern:String; NewPattern:String);&#13;&#10;var i:integer;&#13;&#10;begin&#13;&#10;  i := Pos( OldPattern, SourceString);  // euai OldPattern a oaenoa SourceString&#13;&#10;  while( i &#62; 0 )&#13;&#10;  do&#13;&#10;  begin&#13;&#10;    DeleteStr( SourceString, i, Length(OldPattern) ); // oaaeyai ec oaenoa&#13;&#10;    Insert( NewPattern, SourceString, i );            // anoaaeyai NewPattern SourceString&#13;&#10;    i := Pos( OldPattern, SourceString);              // euai neaao?uaa aoi?aaiea OldPattern a oaenoa SourceString&#13;&#10;  end&#13;&#10;end;&#13;&#10;{1U //  Nicaaiea iia?acaaeaiey&#13;&#10;2U //  Eeeaeaaoey iia?acaaeaiey&#13;&#10;3U //  Nicaaiea ooaoiie iiceoee&#13;&#10;4U //  Eciaiaiea ooaoiie iiceoee&#13;&#10;5U //  Eeeaeaaoey ooaoiie iiceoee&#13;&#10;6U //  Nicaaiea iia?acaaeaiey ia iniiaaiee eeeaeae?iaaiia&#13;&#10;7U //  Nicaaiea ooaoiie iiceoee ia aaca nouanoao?uae&#13;&#10;8U //  Eciaiaiea ia?aiao?ia iia?acaaeaiey&#13;&#10;}&#13;&#10;&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;// Iianoaaeyao no?iee a cai?inao aey na?aa?a MSSQL/ORACLE&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;procedure PrepareQueryForServer();&#13;&#10;var i,j:integer;&#13;&#10;var QR:TfrxODBCQuery;&#13;&#10;var SQL:TStrings;&#13;&#10;var MSSQLTAG, ORACLETAG, SERVERTAG, Str :String;&#13;&#10;begin&#13;&#10;  MSSQLTAG  := '--MSSQL';&#13;&#10;  ORACLETAG := '--ORACLE';&#13;&#10;  SERVERTAG := '--' + GetServerType(); // Oaa oaeouaai na?aa?a&#13;&#10;&#13;&#10;  // i?ioiaeiny ii cai?inai aeeaaee &#34;Aaiiua&#34;&#13;&#10;  for i := 0 to Data.Objects.Count-1 do&#13;&#10;  begin&#13;&#10;    if( UpperCase(Data.Objects[i].ClassName) = 'TFRXODBCQUERY' ) then&#13;&#10;    begin&#13;&#10;      QR  := TfrxODBCQuery(Data.Objects[i]);&#13;&#10;      SQL := QR.SQL;&#13;&#10;      // i?ioiaeiny ii no?ieai cai?ina&#13;&#10;      for j := 0 to SQL.Count-1 do&#13;&#10;      begin&#13;&#10;        Str := SQL.Strings[j];&#13;&#10;        // iiene oaaia MSSQL&#13;&#10;        if( Pos(MSSQLTAG, Str) = 1 ) then&#13;&#10;        begin&#13;&#10;          if( SERVERTAG = MSSQLTAG )&#13;&#10;            then SQL.Strings[j] := Copy(SQL.Strings[j], Length(MSSQLTAG)+1, Length(Str)) // i?eiaiei eiiaiao MSSQL&#13;&#10;            else SQL.Strings[j] := '';                                                   // oaaeei eiiaiao MSSQL&#13;&#10;        end;&#13;&#10;        // iiene oaaia ORACLE&#13;&#10;        if( Pos(ORACLETAG, Str) = 1 ) then&#13;&#10;        begin&#13;&#10;          if( SERVERTAG = ORACLETAG )&#13;&#10;            then SQL.Strings[j] := Copy(SQL.Strings[j], Length(ORACLETAG)+1, Length(Str)) // i?eiaiei eiiaiao ORACLE&#13;&#10;            else SQL.Strings[j] := '';                                                    // oaaeei eiiaiao I?AEEA&#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end&#13;&#10;  end&#13;&#10;end;&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;// Caiaiyao no?ieo a no?iea&#13;&#10;// Ia?aiao?u:&#13;&#10;//           Fio :String            - no?iea iieiiai OEI&#13;&#10;//           LocateInitiale:integer - ?aniiei?aiea eieoeaeia (0- N.A. Eaaiia / 1- Eaaiia N.A.)&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;function GetShortFIO( Fio:String; LocateInitiale : integer ) :string;&#13;&#10;var i, ps :integer;&#13;&#10;var StrFio, LName, FName, PName :string;&#13;&#10;var srcPref :integer;&#13;&#10;begin&#13;&#10;  Result := Fio;&#13;&#10;&#13;&#10;  if( Length(Trim(Fio)) = 0 ) then&#13;&#10;    exit;&#13;&#10;&#13;&#10;  StrFio  := Trim(Fio);&#13;&#10;  LName   := '';&#13;&#10;  FName   := '';&#13;&#10;  PName   := '';&#13;&#10;  srcPref := 1; // Eaaiia N.A.&#13;&#10;&#13;&#10;  if( (Length(StrFio) &#62; 1) and (StrFio[2] = '.') ) then // ecia?aeuii N.A. Eaaiia&#13;&#10;    srcPref := 0;&#13;&#10;&#13;&#10;  // Iaiyai &#60;.&#62; ia &#60; &#62; i?eia?: Eaaiia A.A.&#13;&#10;  ps := Pos('.', StrFio);&#13;&#10;  while(ps &#62; 0 ) do&#13;&#10;  begin&#13;&#10;    StrFio[ps] := ' ';&#13;&#10;    ps := Pos('.', StrFio);&#13;&#10;  end;&#13;&#10;  // Oae?aai aaieiua i?iaaeu&#13;&#10;  ps := Pos('  ', StrFio);&#13;&#10;  while(ps &#62; 0 ) do&#13;&#10;  begin&#13;&#10;    Delete(StrFio, ps, 1);&#13;&#10;    ps := Pos('  ', StrFio);&#13;&#10;  end;&#13;&#10;&#13;&#10;  // Auaaeyai oaieeey eiy io?anoai&#13;&#10;  LName := StrFio;&#13;&#10;  ps := Pos(' ', StrFio);&#13;&#10;  if( ps &#62; 0 ) then&#13;&#10;  begin&#13;&#10;    LName := Copy(StrFio, 1, ps-1);&#13;&#10;    Delete(StrFio, 1, ps);&#13;&#10;    FName := StrFio;&#13;&#10;    ps := Pos(' ', StrFio);&#13;&#10;    if( ps &#62; 0 ) then&#13;&#10;    begin&#13;&#10;      FName := Copy(StrFio, 1, ps-1);&#13;&#10;      Delete(StrFio, 1, ps);&#13;&#10;      PName := StrFio;&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;&#13;&#10;  // Niae?aai ?acoeuoao&#13;&#10;  if( LocateInitiale = 0 ) then // Eieoeaeu aia?aae N.A. Eaaiia&#13;&#10;  begin&#13;&#10;   if (FName&#60;&#62;'') and (PName&#60;&#62;'') then&#13;&#10;    StrFio := IIF(srcPref=1, FName[1], LName[1]) + '.'   // Eiy&#13;&#10;            + IIF(srcPref=1, PName[1], FName[1]) + '. '  // Io?anoai&#13;&#10;            + IIF(srcPref=1, LName, PName)        // Oaieeey&#13;&#10;   else&#13;&#10;      StrFio := LName;&#13;&#10;  end&#13;&#10;  else                          // Eieoeaeu ncaae Eaaiia N.A.&#13;&#10;  begin&#13;&#10;   if (FName&#60;&#62;'') and (PName&#60;&#62;'') then&#13;&#10;    StrFio := IIF(srcPref=1, LName, PName) + ' '   // Oaieeey&#13;&#10;            + IIF(srcPref=1, FName[1], LName[1]) + '.'   // Eiy&#13;&#10;            + IIF(srcPref=1, PName[1], FName[1]) + '.'  // Io?anoai&#13;&#10;   else&#13;&#10;      StrFio := LName;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Result := StrFio;&#13;&#10;end;&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;// Dump Cai?ina n ia?aiao?aie&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;procedure DumpQuery(QR: TfrxODBCQuery);&#13;&#10;  var PT: TFieldType;&#13;&#10;  var PN, PV, PA: String;&#13;&#10;  var i, ps : Integer;&#13;&#10;  var SQL : TStringList;&#13;&#10;  var FileName, PrmKey : String;&#13;&#10;begin&#13;&#10;  if( GetCargoVal('BpI_MrpIS') = 0 ) then  // o eeeaioa ia?aai ioni?eou&#13;&#10;    exit;&#13;&#10;  PrmKey := '_';&#13;&#10;  SQL := TStringList.Create();&#13;&#10;  SQL.Assign(QR.SQL);&#13;&#10;  for i := 0 to QR.Params.Count-1 do&#13;&#10;  begin&#13;&#10;    PN := QR.Params.Items[i].Name;             // Eiy ia?aiao?a&#13;&#10;    PA := ':' + PN;                            // Ia?aiaiiay ia?aiao?a&#13;&#10;    PV := VarToStr(QR.Params.Items[i].Value);  // Cia?aiea ia?aiao?a&#13;&#10;    if QR.Params.Items[i].DataType = ftDate then  // ia?aiao? - aaoa&#13;&#10;      PV := FormatDateTime('yyyy-mm-dd', QR.Params.Items[i].Value);&#13;&#10;&#13;&#10;    PrmKey := PrmKey + PN + '(' + PV + ')_'; //Ee?? ia?aiao?ia&#13;&#10;&#13;&#10;    PV := '''' + PV + ''''; // ia?aieyai a eaau?ee&#13;&#10;&#13;&#10;    ps := Pos( PA, SQL.Text);  // euai ia?aiaiio? ia?aiao?a a oaenoa cai?ina&#13;&#10;    while ps &#62; 0 do&#13;&#10;    begin&#13;&#10;      DeleteStr( SQL.Text, ps, Length(PA) );          // oaaeyai ec oaenoa&#13;&#10;      Insert( PV + '/*' + PN + '*/', SQL.Text, ps );  // caiauaai ia cia?aiea c eiiaioa?eai eiaie ia?aiao?a&#13;&#10;      ps := Pos( PA, SQL.Text ); //&#13;&#10;    end&#13;&#10;  end;&#13;&#10;  FileName := BPRO.BPRO_CTL_STT() + '\\FR3_Dmp_' + QR.Name + '_' + PrmKey + '.sql';&#13;&#10;  FileName := Copy(FileName,1,254);&#13;&#10;  SQL.SaveToFile(FileName);&#13;&#10;  SQL.free();&#13;&#10;end;&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;//----------------------------------------------------------------------------------&#13;&#10;//---------------- END USER FUNC ---------------------------------------------------&#13;&#10;//-----------------------------------------------------------------------------------&#13;&#10;&#13;&#10;{procedure MasterData1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  PAYPRM.Close;&#13;&#10;  PAYPRM.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PAYPRM.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PAYPRM.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  PAYPRM.First;&#13;&#10;  KDRPRM.Close;&#13;&#10;  KDRPRM.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  KDRPRM.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  KDRPRM.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  KDRPRM.First;&#13;&#10;end;}&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  spdr_zp := '';&#13;&#10;  spdr_kdr := '';&#13;&#10;  sse_zp := '';&#13;&#10;  sse_kdr := '';&#13;&#10;  stn_zp := '';&#13;&#10;  stn_kdr := '';&#13;&#10;  sjr_zp := '';&#13;&#10;  sjr_kdr := '';&#13;&#10;  sset_zp := '';&#13;&#10;  sset_kdr := '';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PdrZPOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // Доступ к подразделениям&#13;&#10;  PDRACCZP.Close;&#13;&#10;  PDRACCZP.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PDRACCZP.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PDRACCZP.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  PDRACCZP.ParamByName('IsKdr').Value := 0;&#13;&#10;  PDRACCZP.ParamByName('IsKdr1').Value := 0;&#13;&#10;  PDRACCZP.First;&#13;&#10;  If PDRACCZP.EOF then&#13;&#10;    begin&#13;&#10;      bpdr_zp := true;&#13;&#10;    end&#13;&#10;  else&#13;&#10;    bpdr_zp := false;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PdrKDROnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // Доступ к подразделениям&#13;&#10;  PDRACCKDR.Close;&#13;&#10;  PDRACCKDR.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PDRACCKDR.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  PDRACCKDR.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  PDRACCKDR.ParamByName('IsKdr').Value := 1;&#13;&#10;  PDRACCKDR.ParamByName('IsKdr1').Value := 1;&#13;&#10;  PDRACCKDR.First;&#13;&#10;  If PDRACCKDR.EOF then&#13;&#10;    begin&#13;&#10;      bpdr_kdr := true;&#13;&#10;    end&#13;&#10;  else&#13;&#10;    bpdr_kdr := false;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure StrEdZPOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // Доступ к структурным единицам&#13;&#10;  ACCSEZP.Close;&#13;&#10;  ACCSEZP.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  ACCSEZP.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  ACCSEZP.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  ACCSEZP.ParamByName('IsKdr').Value := 0;&#13;&#10;  ACCSEZP.ParamByName('IsKdr1').Value := 0;&#13;&#10;  ACCSEZP.First;&#13;&#10;  If ACCSEZP.EOF then&#13;&#10;    begin&#13;&#10;      bse_zp := true;&#13;&#10;    end&#13;&#10;  else&#13;&#10;    bse_zp := false;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure StrEdKDROnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // Доступ к структурным единицам&#13;&#10;  ACCSEKDR.Close;&#13;&#10;  ACCSEKDR.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  ACCSEKDR.ParamByName('CdUsr1').Value := &#6