-----------------------------------------------------------------------------------------------------&#13;&#10;&#13;&#10;procedure PrimeDialogOnCloseQuery(Sender: TfrxComponent; var CanClose: Boolean);&#13;&#10;begin&#13;&#10;  IF PrimeDialog.Modalresult = 2 THEN Stopclosereport;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;  LNG := GetCargoval('BPI_LNGWIN'); // 1-rus, 2 -ukr&#13;&#10;&#13;&#10;  //LNG := 1;&#13;&#10;&#13;&#10;  if LNG = 1 then&#13;&#10;    BPRO.SET_LANGUAGE('RU')&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      BPRO.SET_LANGUAGE('UA');&#13;&#10;      Report.ReportOptions.Language := 1;&#13;&#10;    end;&#13;&#10;&#13;&#10;  if LNG = 2 then&#13;&#10;    begin&#13;&#10;      PrimeDialog.Caption := 'Налаштування доступу управління персоналом';&#13;&#10;      Button2.Caption := 'Скасувати';&#13;&#10;      CheckBox0.Caption := 'Загальні параметри';&#13;&#10;      CheckBox1.Caption := 'Підрозділи';&#13;&#10;      CheckBox2.Caption := 'Структурні одиниці';&#13;&#10;      CheckBox3.Caption := 'Діапазон табельних номерів';&#13;&#10;      CheckBox4.Caption := 'Журнали';&#13;&#10;      CheckBox5.Caption := 'Налаштування';&#13;&#10;      CheckBox6.Caption := 'Секретність';&#13;&#10;      CheckBox7.Caption := 'Документи';&#13;&#10;      Label1.Caption := 'Друкувати розділи доступу';&#13;&#10;    end;&#13;&#10;&#13;&#10;  spdr_zp := '';&#13;&#10;  spdr_kdr := '';&#13;&#10;  sse_zp := '';&#13;&#10;  sse_kdr := '';&#13;&#10;  stn_zp := '';&#13;&#10;  stn_kdr := '';&#13;&#10;  sjr_zp := '';&#13;&#10;  sjr_kdr := '';&#13;&#10;  sset_zp := '';&#13;&#10;  sset_kdr := '';&#13;&#10;  if ( (GetUserRepParam( 'SetFormChkbx' )= NULL) OR (GetUserRepParam( 'SetFormChkbx' )= '') ) then&#13;&#10;    iSettings := 255&#13;&#10;  else&#13;&#10;    iSettings := StrToInt( GetUserRepParam( 'SetFormChkbx' ) );&#13;&#10;  USERS.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  //User_Rcd,User_Nm,User_FIO,UserRole_Nm, User_Phn&#13;&#10;  //Создаём таблицу для хранения пользователей&#13;&#10;  USERS_TBL.AddINTField( 'User_Rcd' );//идентификатор записи&#13;&#10;  USERS_TBL.AddSTRINGField( 'User_Nm', 32 );//сетевое имя&#13;&#10;  USERS_TBL.AddSTRINGField( 'User_FIO', 50 );//ФИО&#13;&#10;  USERS_TBL.AddSTRINGField( 'UserRole_Nm', 255 );//роль&#13;&#10;  USERS_TBL.AddSTRINGField( 'User_Job', 255 );//должность&#13;&#10;  USERS_TBL.AddSTRINGField( 'User_Phn', 30 );//телефонный номер&#13;&#10;  USERS_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для журналов з/п&#13;&#10;  JRSZP_TBL.AddINTField( 'Grp' ); // данная запись - группа журналов (1 - true)&#13;&#10;  JRSZP_TBL.AddINTField( 'Grp_Nm' ); // номер группы = UsrJRLs_CdJR&#13;&#10;  JRSZP_TBL.AddSTRINGField( 'JRS_Nm', 50 );//имя журнала&#13;&#10;  JRSZP_TBL.AddINTField( 'JRS_Flg' );//флаги создания/изменения...&#13;&#10;  JRSZP_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для журналов кадров&#13;&#10;  JRSKDR_TBL.AddINTField( 'Grp' ); // данная запись - группа журналов (1 - true)&#13;&#10;  JRSKDR_TBL.AddINTField( 'Grp_Nm' ); // номер группы = UsrJRLs_CdJR&#13;&#10;  JRSKDR_TBL.AddSTRINGField( 'JRS_Nm', 50 );//имя журнала&#13;&#10;  JRSKDR_TBL.AddINTField( 'JRS_Flg' );//флаги создания/изменения...&#13;&#10;  JRSKDR_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для разделов настройки журналов кадров&#13;&#10;  SET_ALL_KDR_TBL.AddINTField( 'SET_ALL_KDR_Cd' ); // номер раздела&#13;&#10;  SET_ALL_KDR_TBL.AddSTRINGField( 'SET_ALL_KDR_Nm', 50 ); // наименование раздела&#13;&#10;  SET_ALL_KDR_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для разделов настройки журналов з/п&#13;&#10;  SET_ALL_ZP_TBL.AddINTField( 'SET_ALL_ZP_Cd' ); // номер раздела&#13;&#10;  SET_ALL_ZP_TBL.AddSTRINGField( 'SET_ALL_ZP_Nm', 50 ); // наименование раздела&#13;&#10;  SET_ALL_ZP_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для отображения настроек доступа&#13;&#10;  SETZP_TBL.AddSTRINGField( 'SETZP_Nm', 50 ); // имя раздела&#13;&#10;  SETZP_TBL.AddINTField( 'SETZP_Flg' ); // флаги чтения/изменения&#13;&#10;  SETZP_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для отображения настроек доступа&#13;&#10;  SETKDR_TBL.AddSTRINGField( 'SETKDR_Nm', 50 ); // имя раздела&#13;&#10;  SETKDR_TBL.AddINTField( 'SETKDR_Flg' ); // флаги чтения/изменения&#13;&#10;  SETKDR_TBL.CreateTable();&#13;&#10;&#13;&#10;  //Создаём таблицу для отображения доступных документов&#13;&#10;  DOCKDR_TBL.AddSTRINGField( 'Doc_Nm', 50 ); // имя раздела&#13;&#10;  DOCKDR_TBL.AddINTField( 'Doc_Flg' ); // флаги чтения/изменения&#13;&#10;  DOCKDR_TBL.CreateTable();&#13;&#10;&#13;&#10;  //создаём таблицу для отображения категорий секретности&#13;&#10;  SekrAccKDR_TBL.AddINTField( 'KdrAccUT_KtS' ); // код&#13;&#10;  SekrAccKDR_TBL.AddSTRINGField( 'KdrAccKtS_Nm', 50 ); // категория секретности&#13;&#10;  SekrAccKDR_TBL.AddINTField( 'KdrAccUT_Tp' ); // код&#13;&#10;  SekrAccKDR_TBL.AddSTRINGField( 'KdrAccTp_Nm', 50 ); // шаблон доступа&#13;&#10;  SekrAccKDR_TBL.AddINTField( 'KdrAccUT_User' ); // код&#13;&#10;  SekrAccKDR_TBL.CreateTable();&#13;&#10;&#13;&#10;  SET_ALL_KDR.First();&#13;&#10;  while ( SET_ALL_KDR.EOF() = false ) do&#13;&#10;  begin&#13;&#10;    SET_ALL_KDR_TBL.Insert();&#13;&#10;    SET_ALL_KDR_TBL.FieldByName( 'SET_ALL_KDR_Cd' ).Value := &#60;SET_ALL_KDR.&#34;Spr_Cd&#34;&#62;;&#13;&#10;    SET_ALL_KDR_TBL.FieldByName( 'SET_ALL_KDR_Nm' ).Value := &#60;SET_ALL_KDR.&#34;Spr_Nm&#34;&#62;;&#13;&#10;    SET_ALL_KDR_TBL.Post();&#13;&#10;    SET_ALL_KDR.Next();&#13;&#10;  end;&#13;&#10;&#13;&#10;  SET_ALL_ZP.First();&#13;&#10;  while ( SET_ALL_ZP.EOF() = false ) do&#13;&#10;  begin&#13;&#10;    SET_ALL_ZP_TBL.Insert();&#13;&#10;    SET_ALL_ZP_TBL.FieldByName( 'SET_ALL_ZP_Cd' ).Value := &#60;SET_ALL_ZP.&#34;Spr_Cd&#34;&#62;;&#13;&#10;    SET_ALL_ZP_TBL.FieldByName( 'SET_ALL_ZP_Nm' ).Value := &#60;SET_ALL_ZP.&#34;Spr_Nm&#34;&#62;;&#13;&#10;    SET_ALL_ZP_TBL.Post();&#13;&#10;    SET_ALL_ZP.Next();&#13;&#10;  end;&#13;&#10;&#13;&#10;&#13;&#10;  while ( USERS.Eof = false ) do&#13;&#10;  begin&#13;&#10;    if ( USERS_TBL.Locate( 'User_Rcd', &#60;USERS.&#34;User_Rcd&#34;&#62;, loCaseInsensitive ) ) then&#13;&#10;    begin&#13;&#10;      USERS_TBL.Edit();&#13;&#10;      USERS_TBL.FieldByName( 'UserRole_Nm' ).Value := USERS_TBL.FieldByName( 'UserRole_Nm' ).Value + ', ' + &#60;USERS.&#34;UserRole_Nm&#34;&#62;;&#13;&#10;    end&#13;&#10;    else&#13;&#10;    begin&#13;&#10;      USERS_TBL.Insert();&#13;&#10;      USERS_TBL.FieldByName( 'User_Rcd' ).Value := &#60;USERS.&#34;User_Rcd&#34;&#62;;&#13;&#10;      USERS_TBL.FieldByName( 'User_FIO' ).Value := &#60;USERS.&#34;User_FIO&#34;&#62;;&#13;&#10;      USERS_TBL.FieldByName( 'User_Nm' ).Value := &#60;USERS.&#34;User_Nm&#34;&#62;;&#13;&#10;      USERS_TBL.FieldByName( 'UserRole_Nm' ).Value := &#60;USERS.&#34;UserRole_Nm&#34;&#62;;&#13;&#10;      USERS_TBL.FieldByName( 'User_Job' ).Value := &#60;USERS.&#34;User_Job&#34;&#62;;&#13;&#10;      USERS_TBL.FieldByName( 'User_Phn' ).Value := &#60;USERS.&#34;User_Phn&#34;&#62;;&#13;&#10;    end;&#13;&#10;&#13;&#10;    USERS_TBL.Post();&#13;&#10;    USERS.Next();&#13;&#10;  end;&#13;&#10;&#13;&#10;&#13;&#10;  //getCargoVal('BPI_SCHEMASQL')+iif(GetServerType='MSSQL', '..', '.')&#13;&#10;  //ShowMessage(getCargoVal('BPI_SCHEMASQL'));&#13;&#10;  //ShowMessage(GetServerType);&#13;&#10;  // Детализация шаблона&#13;&#10;  Secr_Detail.SQL.Text := iif(GetServerType='MSSQL', Secr_Detail_MS.SQL.Text, Secr_Detail_ORA.SQL.Text);&#13;&#10;  //ShowMessage(Secr_Detail.SQL.Text);&#13;&#10;&#13;&#10;end.">
