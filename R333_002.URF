    <TfrxODBCQuery Name="USERS" UserName="USERS" CloseDataSource="true" FieldAliases.Text="" BCDToCurrency="false" IgnoreDupParams="false" SQL.Text="SELECT USR.User_Rcd,User_Nm,User_FIO,User_Job,UserRole_Nm,User_Phn                     &#13;&#10;FROM USER USR      &#13;&#10;LEFT JOIN UserToRole UTR on UTR.User_Rcd = USR.User_Rcd and UTR.UserRole_Firm = :CdFirm                                  &#13;&#10;LEFT JOIN UserRole UR on UR.UserRole_Rcd = UTR.UserRole_Rcd                                    &#13;&#10;ORDER BY User_FIO" PreprocessSQLText="true" SQLZeroCols="false" PropData="05704C65667403A8000470546F70020C0A506172616D657465727301010C39000000204E616D653D2243644669726D222044617461547970653D226674556E6B6E6F776E2220506172616D547970653D227074556E6B6E6F776E220000"/>
