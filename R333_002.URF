') );&#13;&#10;&#13;&#10;  // Закладка ТАБЕЛЬ.&#13;&#10;&#13;&#10;  // Данные табеля&#13;&#10;  Field20.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,9), '-', RuUa('Да','Так') );&#13;&#10;  // Плановые дни и часы за месяц&#13;&#10;  Field21.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,12), '-', RuUa('Да','Так') );&#13;&#10;  // Фактически отработанные дни и часы за месяц&#13;&#10;  Field22.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,13), '-', RuUa('Да','Так') );&#13;&#10;  // Основные сведения лицевых счетов&#13;&#10;  Field23.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,4), '-', RuUa('Да','Так') );&#13;&#10;  // Дни, занятые данными документов&#13;&#10;  Field24.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,10), '-', RuUa('Да','Так') );&#13;&#10;  // Данные закрытых периодов&#13;&#10;  Field25.Text := IIF( GetBitInt(&#60;PAYPRM.&#34;KdrAcc_Prz&#34;&#62;,11), '-', RuUa('Да','Так') );&#13;&#10;&#13;&#10;  spdr_zp := '';&#13;&#10;  spdr_kdr := '';&#13;&#10;  sse_zp := '';&#13;&#10;  sse_kdr := '';&#13;&#10;  stn_zp := '';&#13;&#10;  stn_kdr := '';&#13;&#10;  sjr_zp := '';&#13;&#10;  sjr_kdr := '';&#13;&#10;  sset_zp := '';&#13;&#10;  sset_kdr := '';&#13;&#10;&#13;&#10;  // Доступ к секретности пока что общий для зарплаты и кадров&#13;&#10;  SekrAccKdr.Close;&#13;&#10;  SekrAccKdr.ParamByName('CdUsr').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  //SekrAccKdr.ParamByName('CdUsr1').Value := &#60;USERS_TBL.&#34;User_Rcd&#34;&#62;;&#13;&#10;  SekrAccKdr.ParamByName('CdFirm').Value := GetCargoVal('BpI_CdFirm');&#13;&#10;  SekrAccKdr.First;&#13;&#10;&#13;&#10;  if ( SetZP_TBL.RecordCount() ) then&#13;&#10;    SetZP_TBL.Zap();&#13;&#10;  if ( SetKDR_TBL.RecordCount() ) then&#13;&#10;    SetKDR_TBL.Zap();&#13;&#10;&#13;&#10;  Doc_Kdr_TblBld();&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PrimeDialogOnActivate(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;  i: integer;&#13;&#10;begin&#13