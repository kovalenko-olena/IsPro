<TfrxReport Version="5.4.3" DotMatrixReport="false" IniFile="\Software\Fast Reports" OldStyleProgress="true" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Москалёва Т. Г." ReportOptions.CreateDate="38981.5926213542" ReportOptions.Description.Text="" ReportOptions.Name="FR ЮГОК Табель за первую половину месяца" ReportOptions.LastChange="44992.5032873958" ReportOptions.DirectExportType="deRtf" ReportOptions.DirectExportMode="dmOpen" ReportOptions.DirectExportPages="depSingleSheet" ReportOptions.DirectExportChunkSize="0" ScriptLanguage="PascalScript" ScriptText.Text="//==================================================================&#13;&#10;//  Отчет 253 - Табель учета рабочего времени&#13;&#10;//&#13;&#10;//==================================================================&#13;&#10;var i,i2,LIC,LIC1,Y,z,x:integer;&#13;&#10;var DTUP,LDTUP:TDateTime;&#13;&#10;var masi:array[1..2] of string;&#13;&#10;var mas_save:array[1..2] of string;&#13;&#10;var str_pd,ss,str_vo,strqvo,strqvo1:String;&#13;&#10;var hlf_days,hlf_hrs,full_days,full_hrs,hlf_daysl,hlf_hrsl:integer;&#13;&#10;var k:integer;&#13;&#10;var flgvo:integer;&#13;&#10;var flg_act:integer;&#13;&#10;var str_grppd:String;&#13;&#10;var str_sql :string;&#13;&#10;var mashrs:array[1..9] Of Double;&#13;&#10;var pIsNoTbl:Boolean;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;Function IsNoTbl( pRcd:Integer; pDate:TDateTime ):Boolean;&#13;&#10;var pRet :Boolean;&#13;&#10;begin&#13;&#10;  pRet := False;&#13;&#10;  GrfInt.Close;&#13;&#10;  GrfInt.ParamByName('RCD').Value := pRcd;&#13;&#10;  GrfInt.ParamByName('DTV').Value := pDate;&#13;&#10;  GrfInt.First;&#13;&#10;  If &#60;GrfInt.&#34;Grf_NoTbl&#34;&#62; = 1 then&#13;&#10;    begin&#13;&#10;      pRet := True;&#13;&#10;    end;&#13;&#10;  Result := pRet;&#13;&#10;end;&#13;&#10;&#13;&#10;function DT_STR(D:TDateTime):string;  //(dd.mm.yyyy)&#13;&#10;var dd,mm:string;&#13;&#10;begin&#13;&#10;  If Length(DayOf(D))=1 then&#13;&#10;    begin&#13;&#10;      dd:='0'+IntToStr(DayOf(D));&#13;&#10;    end&#13;&#10;  Else If Length(DayOf(D))=2 then&#13;&#10;    begin&#13;&#10;      dd:=IntToStr(DayOf(D));&#13;&#10;    end;&#13;&#10;  If Length(MonthOf(D))=1 then&#13;&#10;    begin&#13;&#10;      mm:='0'+IntToStr(MonthOf(D));&#13;&#10;    end&#13;&#10;  Else If Length(MonthOf(D))=2 then&#13;&#10;    begin&#13;&#10;      mm:=IntToStr(MonthOf(D));&#13;&#10;    end;&#13;&#10;  Result:=dd+'.'+mm+'.'+IntToStr(YearOf(D));&#13;&#10;end;&#13;&#10;&#13;&#10;// Получить код родительского подразделения для группировки&#13;&#10;function GetGrpCd( pPD:String; pStr:String ):String;&#13;&#10;var tmpStr:String;&#13;&#10;var ResStr:String;&#13;&#10;begin&#13;&#10;  ResStr:='';&#13;&#10;  tmpStr:= pStr;&#13;&#10;  while Pos( ',', tmpStr )&#60;&#62;0 do&#13;&#10;    begin&#13;&#10;      If Copy( tmpStr, 1, Pos( ',', tmpStr )-1 )=Copy( pPD, 1, Length( Copy( tmpStr, 1, Pos( ',', tmpStr )-1 ) ) ) then&#13;&#10;        begin&#13;&#10;          ResStr:=Copy( tmpStr, 1, Pos( ',', tmpStr )-1 );&#13;&#10;//          break;    05.01.2007 wolf&#13;&#10;        end;&#13;&#10;      Delete( tmpStr, 1, Pos( ',', tmpStr ) );&#13;&#10;    end;&#13;&#10;   If tmpStr=Copy( pPD, 1, Length( tmpStr ) ) then&#13;&#10;     begin&#13;&#10;       ResStr:=Copy( tmpStr, 1, Length( tmpStr ) );&#13;&#10;//       break;       05.01.2007 wolf&#13;&#10;     end;&#13;&#10;   Delete( tmpStr, 1, Length( tmpStr ) );&#13;&#10;   Result:= ResStr;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnCloseQuery(Sender: TfrxComponent; var CanClose: Boolean);&#13;&#10;begin&#13;&#10;  If Page2.ModalResult=2 then&#13;&#10;  StopCloseReport;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If flg_act=0 then&#13;&#10;    begin&#13;&#10;      Button4.Visible:=False;&#13;&#10;      Button4.Enabled:=False;&#13;&#10;      RadioButton1.Checked:=True;&#13;&#10;      RadioButton3.Checked:=True;&#13;&#10;      ComboBox3.Items.Clear;&#13;&#10;      ComboBox3.Items.Add(' Час');&#13;&#10;      ComboBox3.Items.Add(' Умовні позначення');&#13;&#10;      ComboBox3.Items.Add(' Коди видів оплат');&#13;&#10;      ComboBox3.Items.Add(' Зміни');&#13;&#10;      ComboBox3.ItemIndex := 0;&#13;&#10;      CheckBox7.Enabled:=True;&#13;&#10;      CheckBox7.Checked:=True;&#13;&#10;      CheckBox10.Enabled:=true;&#13;&#10;      CheckBox10.Checked:=true;&#13;&#10;      If RadioButton1.Checked=True then&#13;&#10;        begin&#13;&#10;          CheckBox1.Enabled:=False;&#13;&#10;          CheckBox2.Enabled:=False;&#13;&#10;          Button3.Enabled:=False;&#13;&#10;          CheckBox1.Checked:=False;&#13;&#10;          CheckBox2.Checked:=False;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          CheckBox1.Enabled:=True;&#13;&#10;          CheckBox2.Enabled:=True;&#13;&#10;          Button3.Enabled:=True;&#13;&#10;        end;&#13;&#10;      If CheckBox3.Checked=False then&#13;&#10;        begin&#13;&#10;          CheckBox4.Enabled:=False;&#13;&#10;          CheckBox4.Checked:=False;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          CheckBox4.Enabled:=True;&#13;&#10;        end;&#13;&#10;      // Вызов из модуля табель&#13;&#10;      If GetCargoVal('Bpi_CdMd') = 37 then&#13;&#10;        begin&#13;&#10;          RadioButton1.Enabled := False;&#13;&#10;          RadioButton2.Enabled := False;&#13;&#10;          Button3.Enabled := False;&#13;&#10;          CheckBox2.Enabled := False;&#13;&#10;          ComboBox1.Enabled := False;&#13;&#10;          ComboBox2.Enabled := False;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          RadioButton1.Enabled := True;&#13;&#10;          RadioButton2.Enabled := True;&#13;&#10;          CheckBox2.Enabled := True;&#13;&#10;          ComboBox1.Enabled := True;&#13;&#10;          ComboBox2.Enabled := True;&#13;&#10;        end;&#13;&#10;      ComboBox1.Items.Clear;&#13;&#10;      ComboBox1.Items.Add(' січень');&#13;&#10;      ComboBox1.Items.Add(' лютий');&#13;&#10;      ComboBox1.Items.Add(' березень');&#13;&#10;      ComboBox1.Items.Add(' квітень');&#13;&#10;      ComboBox1.Items.Add(' травень');&#13;&#10;      ComboBox1.Items.Add(' червень');&#13;&#10;      ComboBox1.Items.Add(' липень');&#13;&#10;      ComboBox1.Items.Add(' серпень');&#13;&#10;      ComboBox1.Items.Add(' вересень');&#13;&#10;      ComboBox1.Items.Add(' жовтень');&#13;&#10;      ComboBox1.Items.Add(' листопад');&#13;&#10;      ComboBox1.Items.Add(' грудень');&#13;&#10;      // Вызов из модуля табель&#13;&#10;      If GetCargoVal('Bpi_CdMd') = 37 then&#13;&#10;        begin&#13;&#10;          ComboBox1.ItemIndex:=MonthOf(GetCargoVal('sPayRep_DatRp'))-1;&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          ComboBox1.ItemIndex:=MonthOf(BPRO.CUR_UP().Value)-1;&#13;&#10;        end;&#13;&#10;      ComboBox2.Items.Clear;&#13;&#10;      i:=1;&#13;&#10;      Y:=1950;&#13;&#10;      For i:=1 To  LIC+5 do&#13;&#10;        begin&#13;&#10;          ComboBox2.Items.Add(' '+IntToStr(Y));&#13;&#10;          Y:=1950+i;&#13;&#10;        end;&#13;&#10;      ComboBox2.ItemIndex:=LIC1;&#13;&#10;    end;&#13;&#10;  flg_act:=1;&#13;&#10;  CheckBox7.Enabled:=True;&#13;&#10;  CheckBox7.Checked:=True;&#13;&#10;//  procedure Page2OnActivate(Sender: TfrxComponent);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If RadioButton1.Checked=True then&#13;&#10;    begin&#13;&#10;      CheckBox1.Enabled:=False;&#13;&#10;      CheckBox2.Enabled:=False;&#13;&#10;      Button3.Enabled:=False;&#13;&#10;      CheckBox1.Checked:=False;&#13;&#10;      CheckBox2.Checked:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      CheckBox1.Enabled:=True;&#13;&#10;      CheckBox2.Enabled:=True;&#13;&#10;      Button3.Enabled:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If RadioButton1.Checked=True then&#13;&#10;    begin&#13;&#10;      CheckBox1.Enabled:=False;&#13;&#10;      CheckBox2.Enabled:=False;&#13;&#10;      Button3.Enabled:=False;&#13;&#10;      CheckBox1.Checked:=False;&#13;&#10;      CheckBox2.Checked:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      CheckBox1.Enabled:=True;&#13;&#10;      CheckBox2.Enabled:=True;&#13;&#10;      Button3.Enabled:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure CheckBox3OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If CheckBox3.Checked=False then&#13;&#10;    begin&#13;&#10;      CheckBox4.Enabled:=False;&#13;&#10;      CheckBox4.Checked:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      CheckBox4.Enabled:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SPRPD.Visible:=False;&#13;&#10;  SPRP.Visible:=False;&#13;&#10;  frmSort.Visible:=False;&#13;&#10;  SetUserRepParam('S_PDR', str_pd );&#13;&#10;  CheckBox7.Enabled:=True;&#13;&#10;  CheckBox7.Checked:=True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button3OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  LDTUP:=EncodeDate(StrToInt(Copy(ComboBox2.Text,2,4)),ComboBox1.ItemIndex+1,DaysInMonth(StrToInt(Copy(ComboBox2.Text,2,4)),ComboBox1.ItemIndex+1));&#13;&#10;  SPRPD.ShowModal();&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button4OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SPRP.ShowModal();&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SPRPDOnResize(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Button6.Anchors := [akRight, akBottom];&#13;&#10;  Button5.Anchors := [akRight, akBottom];&#13;&#10;  Bevel7.Anchors:=[akRight, akBottom,akLeft];&#13;&#10;  Bevel9.Anchors:=[akRight, akBottom,akLeft];&#13;&#10;  CheckBox1.Anchors:=[akRight, akBottom,akLeft];&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SPRPOnResize(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Button8.Anchors := [akRight, akBottom];&#13;&#10;  Button7.Anchors := [akRight, akBottom];&#13;&#10;  CheckListBox2.Anchors:=[akRight, akBottom,akLeft,akTop];&#13;&#10;  Bevel8.Anchors:=[akRight, akBottom,akLeft];&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SPRPDOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Trw1.SpDate := LDTUP;&#13;&#10;  Trw1.CdFirm := GetCargoVal('BPI_CDFIRM');&#13;&#10;  Trw1.CdUser := GetCargoVal('BPI_CDUSR');&#13;&#10;  Trw1.UseAccess := True;&#13;&#10;  Trw1.IsKdr := False;&#13;&#10;  If CheckBox5.Checked then&#13;&#10;    begin&#13;&#10;      Trw1.IncSub := True;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      Trw1.IncSub := False;&#13;&#10;    end;&#13;&#10;  If str_pd &#60;&#62; '' Then&#13;&#10;      Begin&#13;&#10;        Trw1.RePaint;&#13;&#10;        Trw1.SetCheckString(str_pd);&#13;&#10;      End&#13;&#10;  Else&#13;&#10;      Begin&#13;&#10;        Trw1.FillSpr;&#13;&#10;      End;&#13;&#10;  If str_pd='' then&#13;&#10;    begin&#13;&#10;      Button1.Enabled:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      Button1.Enabled:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button5OnClick(Sender: TfrxComponent);&#13;&#10;var g1:integer;&#13;&#10;begin&#13;&#10; str_pd:='';&#13;&#10; str_pd := Trw1.GetCheckString;&#13;&#10; str_grppd := Trw1.GetPdList;&#13;&#10; If str_pd='' then&#13;&#10;    begin&#13;&#10;      Button1.Enabled:=False;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      Button1.Enabled:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure CheckBox1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If CheckBox1.Checked = True Then&#13;&#10;    Begin&#13;&#10;      Trw1.IncSub := True;&#13;&#10;    End&#13;&#10;  Else&#13;&#10;    Begin&#13;&#10;      Trw1.IncSub := False;&#13;&#10;    End;&#13;&#10;  Trw1.SetFocus;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SPRPOnActivate(Sender: TfrxComponent);&#13;&#10;var fkol:integer;&#13;&#10;begin&#13;&#10;  VO.First;&#13;&#10;  while not VO.Eof do&#13;&#10;    begin&#13;&#10;      CheckListBox2.Items.AddObject(&#60;VO.&#34;VO_CDCHR&#34;&#62;+' '+&#60;VO.&#34;SPR_NM&#34;&#62;,TObject(&#60;VO.&#34;VO_CD&#34;&#62;));&#13;&#10;      VO.Next;&#13;&#10;    end;&#13;&#10;  masi[2]:=GetUserRepParam(mas_save[1]);&#13;&#10;  fkol:=CheckListBox2.Items.Count-1;&#13;&#10;  For i:=0 to CheckListBox2.Items.Count-1 do&#13;&#10;    begin&#13;&#10;      CheckListBox2.Checked[i]:=False;&#13;&#10;    end;&#13;&#10;  If masi[2]&#60;&#62;''then&#13;&#10;    begin&#13;&#10;      while Pos(',',masi[2])&#62;0 do&#13;&#10;        begin&#13;&#10;          If fkol&#62;=StrToInt(Copy(masi[2],1,Pos(',',masi[2])-1)) then&#13;&#10;          CheckListBox2.Checked[StrToInt(Copy(masi[2],1,Pos(',',masi[2])-1))]:=True;&#13;&#10;          delete(masi[2], 1, pos(',',masi[2]));&#13;&#10;        end;&#13;&#10;      If fkol&#62;=StrToInt(masi[2]) then&#13;&#10;      CheckListBox2.Checked[StrToInt(masi[2])]:=True;&#13;&#10;    end&#13;&#10;  Else&#13;&#10;    begin&#13;&#10;      If fkol&#62;=0 then&#13;&#10;      CheckListBox2.Checked[0]:=True;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure CheckListBox2OnClick(Sender: TfrxComponent);&#13;&#10;var i,f:integer;&#13;&#10;begin&#13;&#10;  masi[2]:=''; //Очистка элемента массива&#13;&#10;   // Обработка списка&#13;&#10;  for i2 := 0 to (CheckListBox2.Items.Count-1) do&#13;&#10;    begin&#13;&#10;      if CheckListBox2.Checked[i2] then&#13;&#10;      begin&#13;&#10;        masi[2]:=masi[2]+IntToStr(i2)+',';&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;    //Получаем элемент массива с индексами отмеченных записей&#13;&#10;    masi[2]:=Copy(masi[2],1,length(masi[2])-1);&#13;&#10;    SetUserRepParam(mas_save[1], masi[2]);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button7OnClick(Sender: TfrxComponent);&#13;&#10;var g:integer;&#13;&#10;begin&#13;&#10;  str_vo:='';&#13;&#10;  For g:=0 to CheckListBox2.Items.Count-1 do&#13;&#10;    begin&#13;&#10;      If CheckListBox2.Checked[g]=True then&#13;&#10;        begin&#13;&#10;          str_vo:=str_vo+vartostr(CheckListBox2.Items.Objects[g])+',';&#13;&#10;        end;&#13;&#10;    end;&#13;&#10;  str_vo:=Copy(str_vo,1,Length(str_vo)-1);&#13;&#10;  CheckListBox2OnClick(Sender);&#13;&#10;  SetUserRepParam(mas_save[2], str_vo);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var d,d1:integer;&#13;&#10;begin&#13;&#10;  For d1 := 1 To 9 do&#13;&#10;    begin&#13;&#10;      mashrs[d1] := 0;&#13;&#10;    end;&#13;&#10;  // Печатать пустографику&#13;&#10;  If CheckBox3.Checked=True then&#13;&#10;    begin&#13;&#10;      // Включать обозначения выходных и праздничных дней&#13;&#10;      If CheckBox4.Checked=True then&#13;&#10;        begin&#13;&#10;        end&#13;&#10;      Else&#13;&#10;        begin&#13;&#10;          Memo42.Text:='';&#13;&#10;          Memo43.Text:='';&#13;&#10;          Memo44.Text:='';&#13;&#10;          Memo45.Text:='';&#13;&#10;          Memo46.Text:='';&#13;&#10;          Memo47.Text:='';&#13;&#10;          Memo48.Text:='';&#13;&#10;          Memo49.Text:='';&#13;&#10;          Memo50.Text:='';&#13;&#10;          Memo51.Text:='';&#13;&#10;          Memo52.Text:='';&#13;&#10;          Memo53.Text:='';&#13;&#10;          Memo54.Text:='';&#13;&#10;          Memo55.Text:='';&#13;&#10;          Memo56.Text:='';&#13;&#10;          Memo57.Text:='';&#13;&#10;          Memo58.Text:='';&#13;&#10;          Memo59.Text:='';&#13;&#10;          Memo60.Text:='';&#13;&#10;          Memo61.Text:='';&#13;&#10;          Memo62.Text:='';&#13;&#10;          Memo63.Text:='';&#13;&#10;          Memo64.Text:='';&#13;&#10;          Memo65.Text:='';&#13;&#10;          Memo